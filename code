##SODOC##
const collection = require( '../utilities/connection' );
const validator = require( '../utilities/validator' );
let user = {}
let oId=1000;
user.generateId = () => {
    return collection.getCollection().then( ( model ) => {
        return model.aggregate( [{$project: {_id: 0, userId: 1}}, {$sort: {userId: -1}}, {$limit: 1}] ).then( ids=>{
            var number =Number( ids[0].userId.substring( 1 ) );
            return"U"+( number+1 );
        } )
    } )
}




user.clearCart = ( userId ) => {
    // console.log( "in clear cart" )
    return collection.getCollection().then( model => {
        return model.updateOne( { "userId": userId }, { $set: { cart: [] } } ).then( data => {
            if( data.nModified > 0 ) {
                return true;
            }
            else{
               return null;
            }
        } )
    } )
}


//Verify the user credentials and modify the last logout
user.userLogin = ( uEmail, uPass ) => {
    //console.log(uEmail, uPass);
    
    return collection.getCollection().then( userColl => {
        return userColl.find( { "uCredentials.uEmail": uEmail } ).then( data => {
         //   console.log(data);
            if( data.length > 0 ) {
                if( uPass == data[0]['uCredentials']['uPass'] ) {
                    return userColl.updateOne( { "uCredentials.uEmail": uEmail }, { $set: { "uProfile.uLastLogin": new Date().toISOString() } } ).then( res => {
                        if( res.nModified === 1 ) {
                            return data
                        }
                    } )
                } else{
                    let err = new Error( "The password entered is incorrect!!" );
                    err.status = 404;
                    throw err;
                }
            } else{
                let err = new Error( "You are not registered.Please register to login" );
                err.status = 404;
                throw err;
            }
        } )
    } )
}

user.userRegister = ( Person ) => {
    // validator.validateEmail(Person.uEmail);
    // validator.validatePassword(Person.uPass);
    return collection.getCollection().then( userColl => {
        //console.log("Person.uEmail : ",Person.uCredentials.uEmail);
        return userColl.find( { "uCredentials.uEmail": Person.uCredentials.uEmail } ).then( data=>{
            //console.log("Data: ",data);
            if( data.length>0 ){
                let err = new Error( "Sorry this email id is already registered." );
                    err.status = 404;
                    throw err;
            }
            else{
                return user.generateId().then( sid => {
                    Person.userId = sid;
                  //  console.log(sid);
                    return userColl.create( Person ).then( data => {
                        if( data ) {
                            return data;
                        } else{
                            let err = new Error( "Registration Failed! Please try again." );
                            err.status = 404;
                            throw err;
                        }
                    } )
                } )

            }
        } )
       
    } )
}

user.updateCart = (userid, cart) => {
    return collection.getCollection().then(model => {
        return model.updateOne({ "userId": userid },{$set:{cart:cart}}).then(data => {
            if (data.nModified>0) {
           return model.findOne({userId:userid},{_id:0}).then(response=>{
               return response;
           })
            }
            else {
                let err = new Error("Error occurred during cart update. Please try again.");
                err.status = 401;
                throw err;
            }
        })
    })
}
user.add = ( userid, product ) => {
  //  console.log(userid, product);
 //   console.log(product.productId);
    return collection.getCollection().then( model => {
      //  console.log("Hiii");
        return model.findOne( { "userId": userid } ).then( f => {
            if( f ) {
               // console.log(f);
                return model.findOne( { userId: userid, "cart.productId": product.productId } ).then( finded => {
                    if( finded ) {
                   //     console.log("finded", finded);
                        return model.updateOne( { "userId": userid, "cart.productId": product.productId }, { $inc: { "cart.$.quantity": product.quantity } } ).then( updated => {
                           // console.log("updated", updated);
                            if( updated.nModified > 0 ) return updated;
                            else{
                                let err = new Error( "Not Found! Please try again." );
                                err.status = 404;
                                throw err;
                            }
                        } )
                    } else{
                       // console.log("Hello");
                       // console.log(product.sellerName);
                        let obj = {
                            "productId": product.productId,
                            "productName": product.productName,
                            "quantity": product.quantity,
                            "price": product.price,
                            "imgUrl": product.imgUrl,
                            "sellerName": product.sellerName,
                            "discount": product.discount
                        }
                        return model.updateOne( { "userId": userid }, { $push: { cart: obj } } ).then( pushed => {
                            if( pushed.nModified > 0 )
                                return pushed;
                            else{
                                let err = new Error( "Not Pushed! Please try again." );
                                err.status = 404;
                                throw err;
                            }
                        } )
                    }
                } )
            }
            else{
                let err = new Error( "User Not Found! Please try again." );
                err.status = 404;
                throw err;
            }
        } )
    } )
}
user.cartDetails = ( userid ) => {
    return collection.getCollection().then( model => {
        return model.findOne( { "userId": userid }, { cart: 1, _id: 0 } ).then( finded => {
            if( finded ) {
               // console.log(finded, "finded");
                return finded
            } else{
                let err = new Error( "Not Found! Please try again." );
                err.status = 404;
                throw err;
            }
        } )
    } )
}
user.deleteproduct = ( userid, productid ) => {
    return collection.getCollection().then( model => {
        return model.findOne( { "userId": userid } ).then( finded => {
            //console.log(finded);
            if( finded ) {
               // console.log("finded", finded, userid);
                return model.findOne( { userId: userid, "cart.productId": productid }, { "cart.$.productId": 1 } ).then( findp => {
                    if( findp ) {
                     //   console.log("findppppp", findp);
                        return model.update( { userId: userid }, { $pull: { "cart": { "productId": productid } } } ).then( updated => {
                            if( updated ) {
                               // console.log(updated);
                            }
                        } )
                    }
                } )
            }
        } )
    } )
}



user.addOrder = ( userId, successData ) => {
    return collection.getCollection().then( model => {
        return model.findOne( { "userId": userId } ).then( found => {
            if( found ) {
                return model.updateOne( { "userId": userId }, { $push: { "orders": successData } } ).then( updated => {
                    if( updated.nModified > 0 ) {
                        return"Your order has been added to your past orders section. Check for updates."

                    }
                    else{
                        let err = new Error( "Internal server error. Please try after some time!!!" );
                        err.status = 404;
                        throw err;

                    }
                } )
            }
            else{
                let err = new Error( "Internal Server Error!" );
                err.status = 404;
                throw err;
            }
        } )
    } )
}

user.getPastOrders = ( userId ) => {
    return collection.getCollection().then( model => {
      //  console.log("juhgf");
        return model.findOne( { "userId": userId } ).then( found => {
            if( found ) {
              //  console.log("orders:" + found);
                return found;
            }
            else{
                let err = new Error( "You have not added any products yet. Click to add!" );
                err.status = 404;
                throw err;

            }
        } )
    } )
}
user.checkout=( userDetails )=>{
   // console.log("in model: ",userDetails)
return user.clearCart( userDetails.userId ).then( res=>{
    if( res ){
       // console.log("clear cart res: ",res)
       let order=[]
        for( let i=0; i<userDetails.cart.length; i++ ){
        let orderObj={
            "orderId": oId+Math.floor( ( Math.random() * 100 ) + 1 ),
            "productId": userDetails.cart[i].productId,
            "productName": userDetails.cart[i].productName,
            "quantity": userDetails.cart[i].quantity,
            "price": userDetails.cart[i].price,
            "product_img_url": userDetails.cart[i].product_img_url,
            "orderTime": new Date()
        }
        order.push( orderObj );
    }
    // console.log( "Order", order )
        return collection.getCollection().then( model=>{
            return model.updateOne( {userId: userDetails.userId}, {$push: {orders: { $each: order}}} ).then( data=>{
               // console.log("Update data :",data)
                if( data.nModified>0 ){
                    return model.findOne( {userId: userDetails.userId}, {_id: 0} ).then( final=>{
                        // console.log( "Fianal: ", final );
                        if( final ){
                            return final;
                        }
                    } )
               
                }
                else{
                    return null;
                }
            } )
        } )
    
}
else{
    return null;
}
} )
}

user.userdata = ( userid ) => {
    return collection.getCollection().then( model => {
        return model.findOne( { userId: userid }, { _id: 0 } ).then( finduser => {
            if( finduser ) {
                return finduser
            }
            else{
                let err = new Error( "User not Found!" );
                err.status = 404;
                throw err;
            }
        } )
    } )
}
user.updateuser = ( userid, user ) => {
    //console.log("Last",user[])
    return collection.getCollection().then( model => {

        return model.findOne( { userId: userid } ).then( finded => {
            if( finded ) {
               // console.log("finded", finded);
                let obj = {
                    "uProfile.uAddress": user["uAddress"],
                    "uCredentials.uEmail": user["uEmail"],
                    "uProfile.uPhone": user["uPhone"]
                }
              // console.log("obj",obj)
                return model.updateOne( { userId: userid }, { $set: obj } ).then( updated => {
                  //  console.log(updated);
                    if( updated.nModified > 0 ) {
                        return updated;
                    } else{
                        let err = new Error( "User not updated!" );
                        err.status = 404;
                        throw err;
                    }
                } )
            }
        } )
    } )
}
user.updatepassword = ( userid, user ) => {
    return collection.getCollection().then( model => {
        return model.findOne( { userId: userid } ).then( finded => {
            if( finded ) {
                //console.log("New Password in model", user["uPass"]);
                let obj = {
                    "uCredentials.uPass": user["uPass"]
                }
                return model.updateOne( { userId: userid }, { $set: obj } ).then( updated => {
                   // console.log("updated", updated);
                    if( updated.nModified > 0 ) {
                        return updated;
                    } else{
                        let err = new Error( "User password not updated!" );
                        err.status = 404;
                        throw err;
                    }
                } )
            }
        } )
    } )
}
module.exports = user;
