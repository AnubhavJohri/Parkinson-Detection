##SODOC##
import React, { Component } from 'react';
import { connect } from "react-redux";
import { handleEmptyCart, handleUpdateCart } from '../actions/authedUser'
import { isObjectMember } from '@babel/types';
import { BrowserRouter as Router, Route, Switch, Redirect } from 'react-router-dom';
import '../Custom2.css'
import DeleteRoundedIcon from '@material-ui/icons/DeleteRounded';
import BorderColorIcon from '@material-ui/icons/BorderColor';

export class SellerProducts extends Component {
constructor(props) {
    super(props);
    this.state = {
        data: this.props.authedSeller.data[0],
        form: {
            quantity:"" ,
            discount:"",
            price:""
        },
        formValid: {
            quantity: false,
            discount:false,
            price:false,
            isbuttonActive: false
        },
        formErrorMessage: {
            quantity: "",
            discount:"",
            price:""
        },
        updateQty: false,
        updateOb:"",
        successMessage: "",
        errorMessage: "",
        backValid: false,
        addProductValid:false
    }
}

onhandleChange = (event) => {
    const name = event.target.name;
    const value = event.target.value;
    var newForm = this.state.form;
    newForm[name] = value;
    this.setState({ form: newForm })
    this.validateFields(name, value)
}

validateFields = (name, value) => {
    // console.log(name, value);
    var errorMessage = this.state.errorMessage;
    const { formValid, formErrorMessage } = this.state;
    switch (name) {
        case 'quantity':
            if (value.length == 0) {
                formErrorMessage.quantity = "field required";
                formValid.quantity = false;
            }
            else if (value <= 0) {
                formErrorMessage.quantity = "Value can't be less than or equal to 0";
                formValid.quantity = false;
            }
            else {
                formErrorMessage.quantity = "";
                formValid.quantity = true;
            }
            break;

        case 'discount':
            if (value.length == 0) {
                formErrorMessage.quantity = "field required";
                formValid.discount = false;
            }
            else if (value <= 0) {
                formErrorMessage.quantity = "Value can't be less than or equal to 0";
                formValid.discount = false;
            }
            else {
                formErrorMessage.quantity = "";
                formValid.discount = true;
            }
            break;
        
        case 'price':
                if (value.length == 0) {
                    formErrorMessage.price = "field required";
                    formValid.price = false;
                }
                else if (value <= 0) {
                    formErrorMessage.price = "Value can't be less than or equal to 0";
                    formValid.price = false;
                }
                else {
                    formErrorMessage.price = "";
                    formValid.price = true;
                }
            break;
        default:
            break;
    }
    formValid.isbuttonActive = formValid.quantity&&formValid.price&&formValid.discount;
    this.setState({ formValid: formValid, formErrorMessage: formErrorMessage })
}


///TO BE SEEN
deleteItem=(productId)=>{
    var newData=this.state.data;
    var cart=this.state.data.sellerProducts;
    // console.log("my previous cart array=",cart);
    for(let i in cart){
        if(cart[i].productId===productId){
            // console.log("item getting deleted=",i,productId,cart[i].productId,cart[i].productName);
            cart.pop(i);
            // console.log("my updated cart array=",cart);
            break;
        }
    }
    newData.cart=cart;
    this.setState({data:newData});
    handleUpdateCart(this.props.dispatch,this.state.data.sellerId,productId);//This is to update User object in Redux  
}

//Deletes all the products in this.state.data[0].cart
//Later on reflects the foloowing changes in redux store
//Have to push changes to DB at the end of the session
emptyCart = () => {
    var newUser = this.state.data;
    newUser.sellerProducts = []
    this.setState({ data: newUser })
    handleEmptyCart(this.props.dispatch, this.state.data);
}
//Function to Update The quantity in given OrderId 
onUpdateSubmit = (event) => {
    event.preventDefault();
    var newData = this.state.data;
    var cart = this.state.data.sellerProducts;
    for (let i in cart) {
        if (cart[i].productId === this.state.updateOb.productId) {
            // console.log("ob=", i, this.state.updateOb.productId, cart[i].productId);
            cart[i].productQuantity = this.state.form.quantity;
            cart[i].discount = this.state.form.discount;
            cart[i].price = this.state.form.price;
        }
    }
    // console.log("my updated cart array=", cart);
    newData.sellerProducts = cart;
    // console.log("newData", newData.sellerProducts);
    this.setState({ data: newData });
    //handleUpdateCart(this.props.dispatch, this.state.data);//This is to update User object in Redux
    this.setState({ successMessage: "Order Quantity Updated!" });
}

displayUpdateForm = () => {
    return (
        <div className="container">
            <div className="row">
                {this.state.data != undefined && JSON.stringify(this.state.formValid)}
                    {this.state.data != undefined && JSON.stringify(this.state.data.errorMessage)}
                <div className="col-8 offset-3">
                    <div className="card-header">
                    <h1>{"Name:"+this.state.updateOb.productName}</h1>
                    </div>
                    <div className="card-body">
                    <form onSubmit={this.onUpdateSubmit}>
                        <div className="form-group">
                            <label>Quantity to be added:</label><br />
                            <input name="quantity" value={this.state.form.quantity}  onChange={this.onhandleChange} type="number" className="form-control" placeholder="Enter Quantity to be Updated" />
                            <span name="quantityError" className="text-danger">{this.state.formErrorMessage.quantity}</span>
                        </div>
                        <div className="form-group">
                            <label>Price to be Updated:</label><br />
                            <input name="price" value={this.state.form.price} onChange={this.onhandleChange} type="number" className="form-control" placeholder="Enter Price to be Updated" />
                            <span name="priceError" className="text-danger">{this.state.formErrorMessage.price}</span>
                        </div>
                        <div className="form-group">
                            <label>Discount to be Updated:</label><br />
                            <input name="discount" value={this.state.form.discount} onChange={this.onhandleChange} type="number" className="form-control" placeholder="Enter Discount to be Updated" />
                            <span name="discountError" className="text-danger">{this.state.formErrorMessage.discount}</span>
                        </div>
                        <button type="submit" name="update" disabled={!this.state.formValid.isbuttonActive} className="btn btn-success">Update</button>
                        <button type="button" name="Back" className="btn btn-warning" onClick={() => this.setState({ updateQty: false })}>Back</button>
                        <span name="successMessage" className="text-success">
                            <br />
                            <span>{this.state.successMessage}</span>
                            <span name="errorMessage" className="text-danger">{this.state.errorMessage}</span>
                        </span>
                    </form>
                    </div>
                </div>
            </div>
        </div>
    )
}

//Displays all cart PRODUCTS IN CARDS
//DYNAMICALLY POPULATES ALL THE CARDS
displayCards = () => {
    var imgclass = { "height": "150px", "width": "150px" }

    return (
        <React.Fragment>
            {this.state.data != undefined && this.state.data.sellerProducts.length != 0 &&
                <table className="table table-bordered table-striped gfg">
                    <thead>
                        <tr>
                            <th>Name</th>
                            <th>Price</th>
                            <th>Discount</th>
                            <th>Quantity</th>
                            <th>Edit</th>
                            <th>Delete</th>
                        </tr>
                    </thead>
                    <tbody>
                        {
                            this.state.data.sellerProducts.map((ele, key) => {
                                //console.log("elements==",ele);
                                return (
                                    <React.Fragment>
                                        <tr>
                                            <td>{ele.productName}</td>
                                            <td>{ele.price}</td>
                                            <td>{ele.discount}</td>
                                            <td>{ele.productQuantity}</td>
                                            <td><button onClick={() => this.setState({ updateQty: true, updateOb: ele })} className="btn"><BorderColorIcon /></button></td>
                                            <td><button onClick={() => this.deleteItem(ele.productId)} className="btn"><DeleteRoundedIcon /></button></td>
                                        </tr>
                                    </React.Fragment>
                                )
                            })
                        }
                    </tbody>
                </table>
            }
        </React.Fragment>)
}

render() {
    if (this.state.data == undefined) {
        return <Redirect to="/SellerLogin" />
    }
    if(this.state.addProductValid){
        return <Redirect to="/AddProducts" />
    }
    // console.log("this.state.data.sellerProducts=",this.state.data.sellerProducts,this.state.data);
    return (
        <React.Fragment>
            {((this.state.data != undefined) && (this.state.data.sellerProducts.length != 0)) ? ((this.state.updateQty === false) ?
                (
                    <div className="col-8 offset-2 text-center">
                        <h3>{"Hey "+this.state.data.sellerName+" You have " + this.state.data.sellerProducts.length + " Products "}</h3>
                        {this.displayCards()}
                        <button onClick={() => this.emptyCart()} className="btn btn-danger btn-block">Delete All Products</button>
                        <button onClick={() => this.setState({addProductValid:true})} className="btn btn-success btn-block">Add Product</button>
                    </div>
                ) : (<div>{this.displayUpdateForm()}</div>)) :
                (
                    <div className="col-8 offset-2 text-center">
                        <h3>{"Your Cart is Empty"}</h3>
                        <img className="text-center" alt={"Cart is empty"} src="/assets/cartEmpty.png"></img>
                    </div>
                )
            }
        </React.Fragment>
    );
}
}
function mapStateToProps({ authedUser, authedSeller }) {
return {
    authedUser,
    authedSeller
}
}
export default connect(mapStateToProps)(SellerProducts);
