#############################################################################################################################
filepath:Project\hoopla_products-master\src\routes\routing.js##EOFP##
filename:routing.js
##SODOC##
const he=require('hydra-express');
const express=he.getExpress();
const  router=express.Router();
const hydra=require('hydra');
const ob=require('../model/dbsetup');
const productService=require('../service/products');
const productData=require('../model/productData');

//Setting up database
router.get('/setupDb',(req,res,next)=>{
    return ob.setupDb()
    .then(success=>{
        res.json({message:success})
    })
    .catch(err=>{
        next(err);
    })
})

//To get products for different categories
router.get('/getProducts/:productCategory',(req,res,next)=>{
    const productCategory=req.params.productCategory;
    productService.getProducts(productCategory).then(products=>{
        if(products){
            res.json({"data":products});
        }
    }).catch(err=>{
        next(err);
    })
})

//To get all products
router.get('/getAllProducts',(req,res,next)=>{
    productService.getAllProducts().then(products=>{
        if(products){
            res.json({"data":products});
        }
    }).catch(err=>{
        next(err);
    })
})

//Adding a new product (also adding it into seller database)
router.post('/addProduct',(req,res,next)=>{
    var obj= new productData(req.body) ;
   return productService.addProduct(obj).then(success=>{
        if(success){
            let message=hydra.createUMFMessage({
                to:"seller:[POST]/seller/addProduct",
                from:"products",
                body:success
            })
            hydra.makeAPIRequest(message).then(data=>{
               res.json({"data":data});
            })
          }
    }).catch(err=>{
        next(err);
    })
})

//Deleting a product
router.delete('/deleteProduct/:productId', (req, res, next) => {
    const productId=req.params.productId;
    productService.deleteProduct(productId).then(result=>{
        if(result){
            res.json({"message":"Product with productId "+productId+" is deleted"})
        }
    }).catch(err=>{
        next(err);
    })
    

})

//Deleting a seller for a particular product
router.post('/deleteSeller',(req,res,next)=>{
   console.log("Body: ",req.body)
   productService.deleteSeller(req.body).then(result=>{
    if(result){
        res.json({"message":"Seller with sellerID "+req.body.sellerId+" is deleted from Product with id : "+req.body.productId})
    }
}).catch(err=>{
    next(err);
})
})

//Getting seller details for a particular product
router.get('/getSellerDetails/:productId',(req,res,next)=>{
    productService.getSellerDetails(req.params.productId).then(result=>{
        res.json({"message":"Seller details fetched for product with product id : "+req.params.productId,"data":result})
    }).catch(err=>{

        next(err);
    })
})

router.post('/checkout',(req,res,next)=>{
    console.log("In routing of product : ",req.body)
    productService.checkout(req.body).then(result=>{
       // console.log("Response in routing : ",result);
       if(result){
       let message=hydra.createUMFMessage({
        to:"seller:[POST]/seller/checkout",
        from:"products",
        body:{
            "product":result,
            "quantity":req.body.cart[0].quantity
        }
    })
    hydra.makeAPIRequest(message).then(data=>{

       console.log("Final response: ",data);
       res.json({"productMessage":"Product quantity updated successfully."});
    })
}
    
    }).catch((e)=>{
        next(e);
    })
})
module.exports=router;
##EODOC##



------EOD------
#############################################################################################################################
filepath:Project\hoopla_products-master\src\service\products.js##EOFP##
filename:products.js
##SODOC##
const productBookingDb = require('../model/products');
const validator = require('../utilities/Validator');
let productService = {}

productService.getProducts=(productCategory)=>{
    validator.checkproductCategory(productCategory);
    return productBookingDb.getProducts(productCategory).then(products=>{
        if(products){
            return products;
        }else{
            let e = new Error();
            e.message = "No Products found in "+productCategory;
            e.status = 404;
            throw e;
        }
    })
}

productService.getAllProducts=()=>{
    return productBookingDb.getAllProducts().then(products=>{
        if(products){
            return products;
        }else{
            let e = new Error();
            e.message = "No Products found in the Database";
            e.status = 404;
            throw e;
        }
    })
}

productService.addProduct=(productObject)=>{
   
        return productBookingDb.addProduct(productObject).then(ob=>{
            if(ob){
                return ob;
            }else{
                let e = new Error();
                e.message = "Product could not be added";
                e.status = 400;
                throw e;
            }
        })
   
}
productService.deleteSeller=(obj)=>{
    return productBookingDb.deleteSeller(obj).then(ob=>{
        if(ob){
            return ob;
        }else{
            let e = new Error();
            e.message = "Seller is not deleted from product.";
            e.status = 400;
            throw e;
        }
    })

}

productService.deleteProduct=(productId)=>{
    return productBookingDb.deleteProduct(productId).then(result=>{
        if(result){
            return result;
        }else{
            let e = new Error();
            e.message = "Product could not be deleted";
            e.status = 400;
            throw e;
        }
    })
}

productService.getSellerDetails=(productId)=>{
    return productBookingDb.getSellerDetails(productId).then(result=>{
        if(result){
            return result;
        }else{
            let e = new Error();
            e.message = "No seller details found";
            e.status = 400;
            throw e;
        }
    })
}
productService.checkout=(user)=>{
    console.log("In service oof product")
    return productBookingDb.checkout(user).then(result=>{
        //console.log("Response in service of product : ",result)
        if(result){
            return result;
        }else{
            let e = new Error();
            e.message = "Quantity requested is not available right now.";
            e.status = 400;
            throw e;
        }
    })
}
module.exports=productService;
