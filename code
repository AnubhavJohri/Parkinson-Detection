##SODOC##
class Person {
    constructor ( obj ) {
        this.uCredentials = {
            uEmail: obj.uEmail,
            uPass: obj.uPass
        };
        this.uProfile = {
            uName: obj.uName,
            uDOB: obj.uDOB,
            uPhone: obj.uPhone,
            uAddress: obj.uAddress
        };
    }
}

module.exports = Person;
##EODOC##



