##SODOC##
import {
    SET_HP_AUTHED_SELLER,
  } from '../actions/authedSeller'
  

export function authedSeller(state = {data:[],isError:''}, action) {
  // console.log("in reducer",action.hpauthedSeller);
    switch (action.type) {
      case SET_HP_AUTHED_SELLER:
        
        if (action.isError) {
          return {
            data:[],
            isError: action.isError,
          }
        }
        if(action.hpauthedSeller){
          // console.log("in reducer function",action.hpauthedSeller);
          return {
            data:[action.hpauthedSeller],
            isError:''
          }
        }
        else return state
        
  
      default:
        return state
    }
}
