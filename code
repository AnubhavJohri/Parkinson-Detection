#############################################################################################################################
filepath:Project\hoopla_uinew1-master\src\actions\authedSeller.js##EOFP##
filename:authedSeller.js
##SODOC##
import { sellerRegistrationAPI, sellerLoginAPI, addProductsAPI } from '../utils/api'
import { setMessageAction } from './authedUser'

export const SET_HP_AUTHED_SELLER = 'SET_HP_AUTHED_SELLER';


export function setHPAuthedSeller (hpauthedSeller,isError) {

    return {
      type: SET_HP_AUTHED_SELLER,
      hpauthedSeller,
      isError,
    }
  }
  

export const handleSellerRegistration = (dispatch, formData) => {
    //console.log(formData);
    
    return sellerRegistrationAPI(formData).then(smsg => {
        
        dispatch(setMessageAction(smsg.message, ""))
    }).catch(err => {
     dispatch(setMessageAction("", err.message))})
  }
  
  export const handleSellerLogin = (dispatch, formData) => {
    //console.log("---",formData);
    
    return sellerLoginAPI(formData).then(hpauthedSeller => {
        dispatch(setHPAuthedSeller(hpauthedSeller, ""))
    }).catch(err => {
     dispatch(setHPAuthedSeller("", err.message))})
  }

  export const handleAddProduct = (dispatch, formData) => {
    //console.log(formData);
    
    return addProductsAPI(formData).then(smsg => {
        dispatch(setHPAuthedSeller(smsg.data[0],""));
        dispatch(setMessageAction("Product is successfully added!", ""));
    }).catch(err => {
     dispatch(setMessageAction("", err.message))})
  }
##EODOC##



