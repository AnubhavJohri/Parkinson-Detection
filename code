filepath:Project\hoopla_products-master\src\utilities\connection.js##EOFP##
filename:connection.js
##SODOC##
const { Schema } = require("mongoose");
const Mongoose = require("mongoose")
Mongoose.Promise = global.Promise;
Mongoose.set('useCreateIndex', true)
const url = "mongodb://localhost:27017/ProductDB";

const productSchema = Schema({
    productId:{type : String,unique:true,required : [true, 'productId is required']},
    productName:{type:String,required:[true,'productName is required']},
    productManufacturer:{type:String,required:[true,'productManufacturer is required']},
    productImage:{type:String},
    productCategory:{
        type:String,
        enum: 
        {
            values: ['Electronics', 'Shoes', 'Clothing', 'Furniture']
        },  
        required:[true,'product Category is required']},
    productDescription:{type:String,required:[true,'product Description is required']},
    productSpecs:{},
    productRatings:{type:Number,default:5},
   
    sellerDetails:
    [{
        s_id:{type:String,required:[true,'seller id is required']},
        s_name:{type:String,required:[true,'seller Name is required']},
        productDiscount:{type:Number,required:[true,'product discount is required']},
        productPrice:{type:Number,required:[true,'product price is required']},
        productQuantity:{type:Number,required:[true,'product quantity is required']}
    }],
}, {collection : "Products", timestamps: true })


let collection = {};

collection.getProductCollection = () => {
    //console.log("in connection getProDUCTCOLLECTION");
    return Mongoose.connect(url, { useNewUrlParser: true }).then((database) => {
        return database.model('Products', productSchema)
    }).catch(() => {
        let err = new Error();
        err.message="Could not connect to Database";
        err.status = 500;
        throw err;
    })
}

module.exports = collection;
##EODOC##



