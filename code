##SODOC##
import React, { Component } from 'react';
import { connect } from "react-redux";
import { BrowserRouter as Router, Route, Switch ,Redirect} from 'react-router-dom';
import { handleEmptyCart, handleUpdateCart, handlePlaceOrder, handleSendCartOnLogout } from '../actions/authedUser'
import { log } from 'util';
import "../Custom2.css";
export class PastOrders extends Component {
    constructor(props) {
        super(props);
        this.state = {
            data: this.props.authedUser.data[0],
            counter: "",
            msg: ""
        }
    }
    displayCards = () => {
        var imgClass={"max-width":"100%","max-height":"100%",};        
        var imgclass={"height":"150px","width":"150px"}
        return (
            <React.Fragment>
            {this.state.data!=undefined && this.state.data.orders.length != 0 && 
                <table className="table table-striped text-center">
                    <thead className="thead-dark">
                        <tr>
                            <th>Image</th>
                            <th>OrderId</th>
                            <th>Name</th>
                            <th>Quantity</th>
                            <th>Price</th>
                            <th>Order Date</th>
                        </tr>
                    </thead>
                    <tbody>
                        {this.state.data.orders.map((ele, key)=>{
                            // console.log("element=",ele);
                             return (
                                <React.Fragment>
                                    <tr>
                                    <td><img className="card-img-top imageCard" src={ele.product_img_url} alt={ele.productName} /></td>
                                    <td><b>{ele.orderId}</b></td>
                                    <td><b>{ele.productName}</b></td>
                                    <td><b>{ele.quantity}</b></td>
                                    <td><b>&#x20b9;{ele.price}</b></td>
                                    <td><b>{new Date(ele.orderTime).toDateString()}</b></td>
                                    </tr>
                                </React.Fragment>
                            )
                        })}
                    </tbody>
                </table>
            }
        </React.Fragment>
        )
        // return (
        //     this.state.data!=undefined && this.state.data.orders.length != 0 && this.state.data.orders.map((ele, key) => {
        //         // const img_link =require(ele.product_img_url);
        //         return (
        //             <React.Fragment>
        //                 <div className="col-8 offset-2" key={key} id={key}>
        //                     <div className="card bg-light">
        //                         <img className="card-img-top img-fluid" src={ele.product_img_url} alt={ele.productName} />
        //                         <div className="card-body">
        //                             <h3 className="card-title">{ele.productName}</h3>
        //                             <p><b>Quantity:</b> {ele.quantity}</p>
        //                             <p><b>Price:</b> {ele.price}</p>
        //                             {/* <button onClick={()=>this.book(ele)} className="btn btn-custom btn-block">Book</button> */}
        //                         </div>
        //                     </div>
        //                 </div><br />
        //             </React.Fragment>
        //         )
        //     })
        // )
    }
    componentWillUnmount(){
        // console.log("pastOrder did unmount");
        if(this.state!=null && this.state.data!==undefined  &&Object.keys(this.state.data).length!=0 ){
            //console.log("pastorder did unmount",this.state.data.cart);
            var hpAuthedUser=this.state.data
            var stringUserObject=JSON.stringify(hpAuthedUser);
            sessionStorage.setItem("uEmail",stringUserObject);
            handleSendCartOnLogout(this.props.dispatch,this.state.data.cart,this.state.data.userId)
            //handleSendCartOnLogout(this.props.dispatch,this.state.data.cart,this.state.data.userId)
        }
    }

    render() {
        //console.log("im in render in past order",this.state.data);
        if(this.state.data===undefined){
            return <Redirect to="/login"/>
        }
        return (
            <React.Fragment>
                {(this.state.data!=undefined)&&(this.state.data.orders.length!=0)?(<div className="col-8 offset-2 text-center">
                    <h3>{"You have purchased " + this.state.data.orders.length + " Items"}</h3>
                    <div className="gfg">
                    {this.displayCards()}
                    </div>
                    </div>):(
                    <div className="col-8 offset-2 text-center">
                    <h3>{"You haven't purchased any Items!"}</h3>
                    <img className="text-center" alt={"Cart is empty"} src="/assets/cartEmpty.png"></img>
                    </div>)
                }
                
            </React.Fragment>
        );
    }
}
function mapStateToProps({ authedUser }) {
    // console.log("in cart",authedUser);
    return {
        authedUser,
    }
}
export default connect(mapStateToProps)(PastOrders);
