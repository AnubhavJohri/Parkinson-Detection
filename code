##SODOC##
import React, { Component } from 'react';
import { connect } from "react-redux";
import {handleSendCartOnLogout, handleEmptyCart, handleUpdateCart, handleGetSeller, setHPAuthedUser, handleSetHPAuthedUser,handleSetHPAuthedUserCrate } from '../actions/authedUser'
import { isObjectMember } from '@babel/types';
import { BrowserRouter as Router, Route, Switch, Redirect } from 'react-router-dom';
import Rating from '@material-ui/lab/Rating';
import Box from '@material-ui/core/Box';
import '../Custom2.css'
import Button from 'react-bootstrap/Button'
import AddShoppingCartIcon from '@material-ui/icons/AddShoppingCart';
import ButtonToolbar from 'react-bootstrap/ButtonToolbar'

export class ViewProduct extends Component {
constructor(props) {
super(props);
this.state = {
    data:{}  ,
    Sellers:[],
    selectedSellerName:"",
    cart:{
        "productId":"",
        "productName":"",
        "quantity":0,
        "price":0,
        "product_img_url":"",
        "sellerName":"",
        "discount":0,
    },
    formValid:false,
    flag:0,
    counter:0,
    notLoggedATCPressed : false
}}

addToCart=()=>{
    if((this.props.authedUser.data.length > 0)&&(this.props.authedUser.data[0])&&(this.props.authedUser.data[0].uCredentials!=undefined) ){

    var flag=0;
    this.makeCartObject();
    var authedUser=this.props.authedUser.data[0];
    // console.log("product added to cart props=",this.state.cart);
    for(let i in authedUser.cart){
        // console.log("newcarts=",authedUser.cart[i].productName,this.state.cart.productName);
        if(authedUser.cart[i].productName==this.state.cart.productName){
            authedUser.cart[i].quantity+=1;
            flag=1;
            break;
        }
    }
    if(flag==0){
        authedUser.cart.push(this.state.cart)
    }
    //console.log("after",this.state.cart,authedUser);
    handleSetHPAuthedUserCrate(this.props.dispatch,authedUser);
}
else{
    this.setState(()=>({
        notLoggedATCPressed : true
    }))
}
}

makeCartObject=()=>{
    const{productImage,productName,productDescription,productManufacturer,productSpecs,productRatings,productId}=this.props.location.state.data;
    // console.log(this.props.location.state.data);
    var cart=this.state.cart;
    cart["productId"]=productId;
    cart["productName"]=productName;
    cart["product_img_url"]=productImage;
    //sname=this.state.selectedSellerId;
    var flag=0;
    // console.log("sellername=",this.state.selectedSellerName);
    for(let i in this.state.Sellers){
        //console.log("sellers=",this.state.Sellers[i].s_name,this.state.selectedSellerName);
        if(this.state.Sellers[i].s_name===this.state.selectedSellerName){
            // console.log("seller details=",this.state.Sellers[i].s_name,this.state.Sellers[i].productPrice,this.state.Sellers[i].productDiscount,this.state.Sellers[i].productQuantity);
            cart["sellerName"]=this.state.Sellers[i].s_name;
            cart["quantity"]=1;
            cart["price"]=this.state.Sellers[i].productPrice;
            cart["discount"]=this.state.Sellers[i].productDiscount;
            this.setState({cart:cart,flag:1})
        }
    }
}

handleChange=(event)=>{
    const{productImage,productName,productDescription,productManufacturer,productSpecs,productRatings,productId}=this.props.location.state.data;
    var name=event.target.name;
    var value=event.target.value;
    // console.log("name and value=",name,value);
    this.setState({selectedSellerName:value,formValid:true})

}


componentDidMount(){
    this.getAllSellers(this.props.location.state.data.productId);
}


getAllSellers=(productId)=>{
    handleGetSeller(this.props.dispatch,productId).then(arr=>{
        this.setState({Sellers:arr});
    })
}



render() {
    const{productImage,productName,productDescription,productManufacturer,productSpecs,productRatings,productId}=this.props.location.state.data;
    //console.log(this.state.flag==0 && this.state.Sellers.length!=0 && this.state.counter==0);
    // if(this.state.flag==0 && this.state.Sellers.length!=0 && this.state.counter==0 )
    // {
    //     console.log("im in if");
    //     var c=this.state.counter;
    //     c=c+1;
    //     var cart=this.state.cart;
    //     cart["productId"]=productId;
    //     cart["productName"]=productName;
    //     cart["sellerName"]=this.state.Sellers[0].s_name;
    //     cart["quantity"]=this.state.Sellers[0].productQuantity;
    //     cart["price"]=this.state.Sellers[0].productPrice;
    //     cart["discount"]=this.state.Sellers[0].productDiscount;
    //     this.setState({cart:cart,counter:c})
    // }
var specs=[];
for(let prop in productSpecs){
    if(productSpecs[prop]!=undefined &&productSpecs[prop].length!=0 ){
        specs.push({[prop]:productSpecs[prop]})
    }
}

if(this.state.notLoggedATCPressed){
    return <Redirect to = '/login' />
}
return (
    <React.Fragment>
        {/* {JSON.stringify(this.state.cart)} */}
        <div className="container">
            <div className="row marginClass">
                <div className="col-7">
                    <img alt={productName} src={productImage} className="imageClass"></img><br/><br/>
                    <ButtonToolbar>
                    <Button variant="warning" size = "block" disabled={!this.state.formValid} onClick={()=>this.addToCart()} name="addToCart">Add to Cart &nbsp; <AddShoppingCartIcon /></Button>
                    </ButtonToolbar>
                </div>
                <div className="col-5  gfg">
                    <div className="card cardClass1">
                        <div className="card-header text-center"><h1 className="display-4 card-title"><b>{this.props.match.params.productName}</b></h1></div>
                        <div className="card-body ">
                            <b><h4 className="display-5">{"About"}</h4></b>
                            <p>{productDescription}</p>
                            <b><h4 className="display-5">{"Manufactured By:"}</h4></b>
                            <p>{productManufacturer}</p>
                            <b><h4 className="display-5">{"Rating:"}</h4></b>
                            <span><b></b> <Box component="fieldset" mb={3} borderColor="transparent">
                                    <Rating value={productRatings} readOnly />
                                </Box></span>
                            <b><h4 className="display-5">{"Specifications:"}</h4></b>
                            <ul>
                            {specs.map((ele,key)=>{                              
                                return(
                                    <li key={key} id={key}>{Object.keys(ele)[0]+":"}&nbsp;&nbsp;<b>{Object.values(ele)[0]}</b></li>
                                )
                            })}
                            </ul>
                            <b><h4 className="display-5">{"Seller Prices"}</h4></b>
                            <select name="selectedUser" onChange={ this.handleChange} class="form-control">
                            <option value=''>---Select Seller--</option>
                            {/* {this.state.Sellers && this.state.Sellers && this.state.Sellers.length!=0 && this.displayAllSellers()} */}
                            {this.state.Sellers && this.state.Sellers && this.state.Sellers.length!=0 && 
                            this.state.Sellers.map((ele,key)=>{
                                var final_price=ele.productPrice-(ele.productPrice*ele.productDiscount/100);
                                // console.log(ele);
                                return(
                                    <option value={ele.s_name} key={key}>
                                        {ele.s_name+"'s price     "}&#x20b9;{final_price}
                                    </option>
                                )
                            })}
                            </select><br/>
                            {/* <button className="btn btn-danger btn-block" disabled={!this.state.formValid} onClick={()=>this.addToCart()}>Add to Cart</button>             */}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </React.Fragment>
);
}
}
function mapStateToProps({ authedUser, authedSeller }) {
return {
authedUser,
authedSeller
}
}
export default connect(mapStateToProps)(ViewProduct);
