let create = {}

create.setupDB = () => {
    return collection.getCollection().then( userColl => {
        return userColl.deleteMany().then( data => {
            // console.log( data );
            return userColl.insertMany( userData ).then( result => {
                if( result && result.length > 0 ) return result.length
                else return null
            } );
        } );
    } );
}

module.exports = create
