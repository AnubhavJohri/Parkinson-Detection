##SODOC##
/**
* REDUCERS related to authenticated users
*/
import {
  SET_HP_AUTHED_USER, SET_MESSAGE, SET_HP_ALL_PRODUCTS,
} from '../actions/authedUser'


export function allProducts(state = {data:[],isError:''}, action){
  // console.log("in reducer function alllProducts");
  switch(action.type){
    case SET_HP_ALL_PRODUCTS:
      if(action.isError){
        return{
          data:[],
          isError: action.isError,
        }
      }
        if(action.allProducts){
          // console.log(action.allProducts);
          
          return {
            data:[action.allProducts],
            isError:''
          }
        }
        else return state;
      default:
        return state
  }
  
}


export function authedUser(state = {data:[],isError:''}, action) {
  // console.log(action);
  
  switch (action.type) {
    case SET_HP_AUTHED_USER:
      
      if (action.isError) {
        return {
          data:[],
          isError: action.isError,
        }
      }
      if(action.hpAuthedUser){
        return {
          data:[action.hpAuthedUser],
          isError:''
        }
      }
      else return state
      

    default:
      return state
  }
}

export const Messages = (state = { successMessage: "", errorMessage: '' }, action) => {
  /* your code goes here */
  if(action.type==SET_MESSAGE){
    // console.log(action.successMessage, action.errorMessage)
      return ({
    
          successMessage: action.successMessage,
          errorMessage: action.errorMessage
      })
  
  
  }
  else{
      return state
  }
}
