filepath:Project\hoopla_products-master\src\model\productData.js##EOFP##
filename:productData.js
##SODOC##
class productData {
    constructor(product) {
        this.productId=0;
        this.productName = product.productName;
        this.productManufacturer=product.productManufacturer;
        this.productCategory=product.productCategory;
        this.productDescription=product.productDescription;
        this.productRatings=product.productRatings;
        this.productSpecs=product.productSpecs;
        this.productImage=product.productImage;
       this.sellerDetails=[{
           s_name:product.s_name,
           s_id:product.s_id,
           productDiscount:product.productDiscount,
           productPrice:product.productPrice,
           productQuantity:product.productQuantity
       }]
        
    }
}
module.exports = productData
