const he = require( 'hydra-express' );
const Hexpress = he.getExpress();
let routing = Hexpress.Router();
 const hydra=require( 'hydra' )
const service = require( '../service/user' );
const Person = require( '../model/person' );
// const Product = require('../model/product')

//To verify the credentials of user
routing.post( '/login', ( req, res, next ) => {
    let uEmail = req.body.uEmail;
    let uPass = req.body.uPass;
    //console.log(uEmail);
   // console.log(uPass);
    return service.loginUser( uEmail, uPass ).then( item => {
     //   console.log(item);
        res.json( { data: item } );
    } ).catch( err => {
        next( err );
    } );
} );

routing.post( '/register', ( req, res, next ) => {
    //console.log("USER REGISTER")
    let newUser = new Person( req.body );
   // console.log(req.body);
    return service.registerUser( newUser ).then( data => {
        //console.log(data)
        res.json( {"message": "You have been registered successfully!"} );
    } ).catch( err => {
        next( err );
    } );
} );



routing.put( '/clearCart/:userId', ( req, res, next ) => {
    let userId = req.params.userId;
   // console.log(userId)
    return service.clearCart( userId ).then( data => {
        res.json( data );
    } ).catch( err => {
        next( err );
    } )
} )



routing.post( '/addOrder/:userId', ( req, res, next ) => {
    let userId = req.params.userId;
    let successData = req.body;
    return service.addOrder( userId, successData ).then( data => {
        res.json( { "message": data } );
    } ).catch( err => {
        next( err );
    } )
} )

routing.get( '/getPastOrders/:userId', ( req, res, next ) => {
   // console.log("atrayee routing1");
    let userId = req.params.userId;
    return service.getPastOrders( userId ).then( data => {
       // console.log("atrayee routing2");
        res.json( {"past": data} );
    } ).catch( err => {
        next( err );
    } );
} );
routing.put("/updateCart/:userid", (req, res, next) => {
    let userid = req.params.userid;
    let cart = req.body;
    return service.updateCart(userid,cart).then(details => {
        res.json({"data":details})
    }).catch(err => {
        next(err);
    });
});
routing.put( "/addtocart/:userid", ( req, res, next ) => {
    let userid = req.params.userid;
   // console.log(req.body);
   // console.log(userid); 
    return service.add( userid, req.body ).then( details => {
       // console.log(details);
        res.json( { "message": "Added to cart successfully" } )
    } ).catch( err => {
        next( err );
    } );
} );
routing.get( "/cartdetails/:userid", ( req, res, next ) => {
    let userid = req.params.userid;
    return service.cartDetails( userid ).then( data => {
        res.json( data );
    } ).catch( err => {
        next( err );
    } )
} )
routing.get( "/deletedata/:userid/:productid", ( req, res, next ) => {
    let productid = req.params.productid;
    let userid = req.params.userid;
    return service.deleteproduct( userid, productid ).then( data => {
       // console.log(data);
        res.json( { "message": "Deleted Successfully" } );
    } ).catch( err => {
        next( err );
    } )
} )

routing.post( '/checkout', ( req, res, next )=>{
    //console.log(req.body)
 return service.checkout( req.body ).then( userData=>{
    let message=hydra.createUMFMessage( {
        to: "products:[POST]/products/checkout",
        from: "products",
        body: req.body
    } )
    hydra.makeAPIRequest( message ).then( data=>{
    //    console.log( "Data returned: ", data )
       res.json( {userData} );
    } )

   //  res.json({"message":"Order placed successfully."})
 } ).catch( ( e )=>{
     next( e );
 } )
} )


routing.get( "/userdetails/:userid", ( req, res, next ) => {
    let userid = req.params.userid;
    return service.userdata( userid ).then( data => {
        res.json( {"user": data} )
    } ).catch( err => {
        next( err );
    } )
} )
routing.put( "/updateuser/:userid", ( req, res, next ) => {
    let userid = req.params.userid;
    //console.log("Start",req.body);
    return service.updateuser( userid, req.body ).then( data => {
        //console.log(data);
        res.json( {"message": "Updated Data Successfully"} )
    } ).catch( err => {
        next( err );
    } )
} )
routing.put( "/updatepassword/:userid", ( req, res, next ) => {
    let userid = req.params.userid;
    //console.log(req.body)
    return service.updatepassword( userid, req.body ).then( data => {
        //console.log(data);
        res.json( {"message": "Updated password Successfully"} )
    } ).catch( err => {
        next( err );
    } )
} )
module.exports = routing
