filename:NavBar.js
##SODOC##
import React, { Component } from 'react';
import { Link } from 'react-router-dom';
import Navbar from 'react-bootstrap/Navbar'
import Button from 'react-bootstrap/Button'
import Form from 'react-bootstrap/Form'
import FormControl from 'react-bootstrap/FormControl'
import Nav from 'react-bootstrap/Nav'
import ShoppingCartTwoToneIcon from "@material-ui/icons/ShoppingCartTwoTone";
import { handleSearchCategory, handleSetHPAuthedUser, handleSetHPAuthedUserCrate } from '../actions/authedUser';
import { connect } from "react-redux";
import { BrowserRouter as Router, Route, Switch, Redirect } from 'react-router-dom';
import { Badge } from '@material-ui/core';
import './custom.css'
import LocalMallIcon from '@material-ui/icons/LocalMall';
import AccountCircleIcon from '@material-ui/icons/AccountCircle';
import ExitToAppIcon from '@material-ui/icons/ExitToApp';
import StoreMallDirectoryIcon from '@material-ui/icons/StoreMallDirectory';

class NavBar extends Component {
  constructor(props) {
    super(props);
    this.state = {
      data: "",
      productCategory: "",
      productData: "",
      redirect: false,
      totalCartItems: 0,
      totalCartPrice: 0,
      logoutPressed: false,

    }
  }

  onhandleChange = (event) => {
    const name = event.target.name;
    const value = event.target.value;
    var st = "";
    if (value.length != 0) {
      st = value[0].toUpperCase() + value.slice(1);
    }
    this.setState({ productCategory: st })

  }

  onSearch = (event) => {
    event.preventDefault();
    this.setState(() => ({
      redirect: true
    }))
  }

  // componentDidUpdate() {
  //   if (this.state.redirect) {
  //     this.setState({
  //       redirect: false
  //     })

  //   }
    

  // }


  getTotalCartPrice = () => {
    this.setState({ totalCartItems: 0, data: this.props.authedUser.data[0] })
    var totalPrice = 0;
    let userLoggedIn = this.props.authedUser.data.length;

    var finalPrice = 0;
    // console.log("i worked", this.props.authedUser.data[0].cart);
    var qty = 0;
    if ((userLoggedIn > 0)&&(this.props.authedUser.data[0])&&(this.props.authedUser.data[0].uCredentials!=undefined) )
      for (let i in this.props.authedUser.data[0].cart) {
        finalPrice = this.props.authedUser.data[0].cart[i].price - this.props.authedUser.data[0].cart[i].price * (this.props.authedUser.data[0].cart[i].discount / 100);
        totalPrice = totalPrice + (finalPrice * this.props.authedUser.data[0].cart[i].quantity);
        // console.log("quantity of i=", this.props.authedUser.data[0].cart[i].quantity, qty);
        qty = Number(this.props.authedUser.data[0].cart[i].quantity) + Number(qty);
      }
    // console.log("quantity=", qty);
    this.setState({ totalCartItems: qty, totalCartPrice: totalPrice });
  }

  componentDidUpdate = (prevProps) => {
    if (prevProps != this.props) {
      this.getTotalCartPrice();
      // console.log("in didupdate navbar", this.props.authedUser);
    }

    else if (this.state.redirect) {
      this.setState({
        redirect: false
      })

    }
  }

  componentDidMount = () => {
    var userObject = sessionStorage.getItem("uEmail");
    // console.log("in didmount navbar", userObject);
    if (userObject !== undefined || userObject !== null) {
      userObject = JSON.parse(userObject);
      // console.log("in didmount navbar creating object", userObject);
      handleSetHPAuthedUserCrate(this.props.dispatch, userObject);
    }
    
  }

  handleLogout = () => {
    // console.log("handleLgout");
    sessionStorage.setItem('uEmail', "");
    handleSetHPAuthedUserCrate(this.props.dispatch, [])
    this.setState(() => ({
      logoutPressed: true,
      
    }))


  }

  logoutIssue = () => {
    this.setState(()=>({
      logoutPressed : false
    }))
  }

  render() {
    let noOfCartItems = "";
    let userLoggedIn = this.props.authedUser.data.length;
    let sellerLoggedIn = this.props.authedSeller.data.length;
  
    (userLoggedIn > 0) ? (noOfCartItems = this.state.totalCartItems) : (noOfCartItems = 0)
    const redirect = this.state.redirect;
    const logoutPressed = this.state.logoutPressed;
    if (redirect) {
      return <Redirect to={'/search/' + this.state.productCategory} />
    }

    else if (logoutPressed) {
      //console.log("before this.state",this.state.logoutPressed);
      // this.setState(()=>({
      //   logoutPressed : false
      // }))
      // console.log("======",this.props.authedUser.data[0].length);
      // console.log("======",this.props.authedUser.data[0].uCredentials);
      // console.log("after this.state", this.state.logoutPressed);
      this.logoutIssue();
      return <Redirect to='/login' />
    }
    // console.log("userLoggedIn=", userLoggedIn);

    
    

    return (

      <React.Fragment>
        <Navbar style={{ backgroundColor: "#000000", boxShadow: "1px 1px 1px grey" }} >
          <Link to='/dashboard'>
            <Navbar.Brand style={{ color: "white" }}>Hoopla</Navbar.Brand>
          </Link>
          <Form inline onSubmit={this.onSearch}>
            <FormControl type="text" placeholder="Search" name="productCategory" onChange={this.onhandleChange} className="mr-sm-2" />
            <Button variant="outline-info" type="submit" style={{ color: "white" }} type="submit">Search</Button>
          </Form>

          {((userLoggedIn > 0)&&(this.props.authedUser.data[0])&&(this.props.authedUser.data[0].uCredentials!=undefined) )? (<Nav className="navLinks nav-link ml-auto" style={{ color: "white" }}>
            Welcome, {this.props.authedUser.data[0].uProfile.uName}!</Nav>) : (
              (sellerLoggedIn > 0) ? 
              (<Nav className="navLinks nav-link ml-auto" style={{ color: "white" }}>
              Welcome, {this.props.authedSeller.data[0].sellerName}!</Nav>)
             : (
              <Nav className="navLinks nav-link  ml-auto" style={{ color: "white" }}>
                Welcome, Guest!</Nav>
            ))} 
          
          {((userLoggedIn > 0)&&(this.props.authedUser.data[0])&&(this.props.authedUser.data[0].uCredentials!=undefined) ) ? (null) : ((sellerLoggedIn > 0) ? (
            <Nav style={{ color: "white" }}>
            <Link to='/SellerProducts' className="navLinks nav-link" style={{ color: "white" }}>
              <StoreMallDirectoryIcon />
            </Link></Nav>
          ) : (
            <Nav style={{ color: "white" }}>
            <Link to='/login' className="navLinks nav-link" style={{ color: "white" }}>
              <AccountCircleIcon />
            </Link></Nav>
          ))}


          
          {((userLoggedIn > 0)&&(this.props.authedUser.data[0])&&(this.props.authedUser.data[0].uCredentials!=undefined) )? (<Nav><Link to='/cart' className="navLink nav-link" style={{ color: "white" }}>
            &nbsp;  &nbsp;
              <Badge badgeContent={noOfCartItems} color="secondary">
              <ShoppingCartTwoToneIcon />
            </Badge>

            &nbsp; &nbsp;
          </Link></Nav>) : (null)}
          {((userLoggedIn > 0)&&(this.props.authedUser.data[0])&&(this.props.authedUser.data[0].uCredentials!=undefined) ) ? (<Nav>
            <Link to='/PastOrders' className="navLinks nav-link" style={{ color: "white" }}>
              <LocalMallIcon />
            </Link></Nav>) : (null)}

            &nbsp; &nbsp;
          {((userLoggedIn > 0)&&(this.props.authedUser.data[0]) &&(this.props.authedUser.data[0].uCredentials!=undefined)) ? (<Nav>
            <Link onClick={this.handleLogout} className="navLinks nav-link" style={{ color: "white" }}><ExitToAppIcon /></Link>

          </Nav>) : (null)}
        </Navbar>

      </React.Fragment>
    );
  }
}
function mapStateToProps({ authedUser, allProducts, authedSeller }) {
  return {
    authedUser,
    allProducts,
    authedSeller

  }
}
export default connect(mapStateToProps)(NavBar);
