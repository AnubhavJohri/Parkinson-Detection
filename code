filepath:Project\hoopla_products-master\src\model\products.js##EOFP##
filename:products.js
##SODOC##
const dbModel = require('../utilities/connection');
const productDb = {};

productDb.generateId = () => {
    return dbModel.getProductCollection().then((model) => {
        return model.aggregate([{$project:{_id:0,productId:1}},{$sort:{productId:-1}},{$limit:1}]).then(ids=>{
            var number =Number(ids[0].productId.substring(1,));
            return "P"+(number+1);
        })
    })
}

productDb.getProducts=(productCategory)=>{   
    return dbModel.getProductCollection().then(product=>{
        return product.find({productCategory:productCategory},{_id:0}).then(success=>{

            if(success.length!=0){
                return success;
            }else{
                return null;
            }
        })
    })
}

productDb.getAllProducts=()=>{
    return dbModel.getProductCollection().then(product=>{
        return product.find({},{_id:0}).then(success=>{
            if(success.length!=0){
                return success;
            }else{
                return null;
            }
        })
    })
}

productDb.addProduct=(productObject)=>{
  
    return dbModel.getProductCollection().then(product=>{
        
        return product.find({ $and: [ { productName: productObject.productName }, { productManufacturer: productObject.productManufacturer} ] },{_id:0}).then(data=>{
          
            if(data.length>0){
                productObject.productId=data[0].productId;
                return product.updateMany({productName:productObject.productName,productManufacturer:productObject.productManufacturer},{$push:{sellerDetails:productObject.sellerDetails[0]}}).then(data1=>{
                  
                    if(data1.nModified>0){
                        return productObject;
                    }
                    else{
                        return null;
                    }
                })
            }
            else{
                return productDb.generateId().then(id=>{
                    productObject.productId=id;
                   
                     return product.create(productObject).then(ob=>{
                    if(ob){
                       
                        return ob;
                    }else{
                        return null;
                    }
                })
            })
            }
        })
       
    })

}


productDb.deleteProduct=(productId)=>{
    
    return dbModel.getProductCollection().then(product=>{
        return product.deleteOne({productId:productId}).then(acknowledged=>{
            if(acknowledged.deletedCount==1){
                return 1;
            }else{
                return null;
            }
        })
    })
}

productDb.deleteSeller=(obj)=>{
    const {productId,sellerId}=obj;
    return dbModel.getProductCollection().then(product=>{
        return product.updateOne({productId:productId},{$pull:{"sellerDetails":{"sellerId":sellerId}}}).then(acknowledged=>{
            if(acknowledged.nModified>0){
                return true;
            }else{
                return null;
            }
        })
    })
}

productDb.getSellerDetails=(productId)=>{
    return dbModel.getProductCollection().then(model=>{
        return model.find({productId:productId},{_id:0,sellerDetails:1}).then(data=>{
            if(data.length>0){
                return data;
            }
            else{
                return null;
            }
        })
    })
}
productDb.checkout=(user)=>{
  
    return dbModel.getProductCollection().then(model=>{
           let i=0;
            return model.find({$and:[{productId:user.cart[i].productId},{"sellerDetails.s_name":user.cart[i].sellerName}]},{_id:0,"sellerDetails.productQuantity":1}).then(udata=>{
               // console.log(udata);
                if(udata.length>0 && udata[0].sellerDetails[0].productQuantity>= user.cart[i].quantity){
                   // console.log("quantity: ",udata);
                    return model.updateOne({$and:[{productId:user.cart[i].productId},{"sellerDetails.s_name":user.cart[i].sellerName}]},{$inc:{"sellerDetails.$.productQuantity":-user.cart[i].quantity}}).then(data=>{
                       // console.log("Data here:",data)
                      if(data.nModified>0){
                          console.log("Product update : ",data);
                          return model.aggregate([{$unwind:"$sellerDetails"},{$match:{$and:[{productId:user.cart[i].productId},{"sellerDetails.s_name":user.cart[i].sellerName}]}},{$project:{_id:0,"sellerDetails.s_id":1,productId:1}}]).then(sid=>{
                              if(sid){
                                  console.log(sid);
                                  return sid;
                              }
                          })
                      }
                      else{
                          return null;
                      }
                    })

                }
                else{
                    return null;
                   
                }
            })
           
        // }
        // if(v>0){
        //     return true;
        // }
        // else{
        //     return null;
        // }
        
    })
}

module.exports=productDb;
##EODOC##



