##SODOC##
const validator = {};

validator.checkSId = function (sid){
    if(!sid.match(/[S][0-9]{4}/)){
        let error = new Error('Seller Id not in format (S1001)');
        error.status = 406;
        throw error;
    }
}

validator.checkAccNo=(acno)=>{
    if(acno.length<9 || acno.length>18){
        let error = new Error("Account Number must be 9-18 characters");
        error.status = 406;
        throw error;
    }
}
validator.checkTAN=(TAN)=>{
    if(!TAN.match(/^[A-Z]{4}[0-9]{5}[A-Z]{1}$/)){
        let error = new Error("The TAN should be of format: AAAA12345A");
        error.status = 406;
        throw error;
    }
}
validator.checkGST=(GST)=>{
    if(!GST.match(/^[0-9]{2}[A-Z]{5}[0-9]{4}[A-Z]{1}[0-9]{1}[A-Z]{1}[0-9]{1}$/)){
        let error = new Error("The GST should be of format:12ABCDE1234A1Z1");
        error.status = 406;
        throw error;
    }
}


module.exports = validator;
