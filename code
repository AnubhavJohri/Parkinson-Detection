filename:validator.js
##SODOC##
let Validator = {};

Validator.validateEmail = function ( email ) {
    let str = /^[a-z]*@[a-z]+\.com$/;
    if( !str.test( email ) ) {
        let error = new Error( 'Wrong Email' );
        error.status = 406;
        throw error;
    }
}


Validator.validatePassword = function ( c ) {
    let reg1 = /[A-Z]/;
    let reg2 = /[a-z]/;
   let reg3 = /[0-9]/;
   let reg4 = /[!@#$%^&*]/;
   if( !reg1.test( c ) || !reg2.test( c ) || !reg3.test( c ) || !reg4.test( c ) ) {
        let error = new Error( 'Password not strong' );
        error.status = 406;
        throw error;
    }
}

module.exports = Validator;
##EODOC##


