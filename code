##SODOC##
import React, { Component, Fragment } from 'react';
import Avatar from '@material-ui/core/Avatar';
import Button from '@material-ui/core/Button';
import CssBaseline from '@material-ui/core/CssBaseline';
import Paper from '@material-ui/core/Paper';

import TextField from '@material-ui/core/TextField';
import FormControlLabel from '@material-ui/core/FormControlLabel';
import Checkbox from '@material-ui/core/Checkbox';
// import Link from '@material-ui/core/Link';
import StoreIcon from '@material-ui/icons/Store';
import Grid from '@material-ui/core/Grid';
import Box from '@material-ui/core/Box';
import Typography from '@material-ui/core/Typography';
import { makeStyles } from '@material-ui/core/styles';
import Container from '@material-ui/core/Container';
import { handleSellerRegistration } from '../actions/authedSeller';
import { connect } from 'react-redux'
import PropTypes from 'prop-types';
import withStyles from '@material-ui/core/styles/withStyles';
import SellerLogin from './SellerLogin';
import { Link } from 'react-router-dom'
import { setMessageAction } from '../actions/authedUser';





const styles = theme => ({
    '@global': {
        body: {
            backgroundColor: theme.palette.common.white,
        },
    },
    paper: {
        marginTop: theme.spacing(8),
        display: 'flex',
        flexDirection: 'column',
        alignItems: 'center',
    },
    avatar: {
        margin: theme.spacing(1),
        backgroundColor: 'black',
    },
    form: {
        width: '100%', // Fix IE 11 issue.
        marginTop: theme.spacing(3),
        
    },
    submit: {
        margin: theme.spacing(3, 0, 2),
    },
});


class Register extends Component {
    constructor(props) {
        super(props);
        this.state = {
            form: {
                name: "",
                mNumber: "",
                TAN: "",
                acNumber: "",
                email: "",
                password: "",
                GST: ""
            },
            formErrorMessage: {
                name: "",
                GST: "",
                mNumber: "",
                TAN: "",
                acNumber: "",
                email: "",
                password: "",
            },
            formValid: {
                name: false,
                GST: false,
                mNumber: false,
                TAN: false,
                acNumber: false,
                email: false,
                password: false,
                buttonActive: false,
            },
        }
    }

    Copyright = () => {
        return (
            <Typography variant="body2" color="textSecondary" align="center">
                {'Copyright © '}
                <Link color="inherit" href="/dashboard">
                    Hoopla
      </Link>{' '}
                {new Date().getFullYear()}
                {'.'}
            </Typography>
        );
    }


    handleChange = (event) => {
        const value = event.target.value;
        const name = event.target.name;
        this.setState((prevState) => ({
            form: {
                ...prevState.form,
                [name]: value
            }
        }))
        this.validateField(name, value)
    }

    //formValidations
    validateField = (fieldName, value) => {
        const newFormErrorMsg = this.state.formErrorMessage;
        const newFormValid = this.state.formValid;

        switch (fieldName) {
            case 'name':
                if (value == "") {
                    newFormErrorMsg.name = "Field required"
                    newFormValid.name = false
                }

                else if (!value.match(/^[a-zA-Z]+([ ]*[a-zA-Z]+)*$/)) {
                    newFormErrorMsg.name = "Name can contain only alphabets and spaces"
                    newFormValid.name = false
                }
                else {
                    newFormErrorMsg.name = ""
                    newFormValid.name = true
                }
                break;

            case 'email':
                if (value == "") {
                    newFormErrorMsg.email = "Field required"
                    newFormValid.email = false
                }

                else if (!value.match(/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/)) {
                    newFormErrorMsg.email = "Email Id must be of format John@example.com"
                    newFormValid.email = false
                }
                else {
                    newFormErrorMsg.email = ""
                    newFormValid.email = true
                }
                break;

            case 'acNumber':
                if (value == "") {
                    newFormErrorMsg.acNumber = "Field required"
                    newFormValid.acNumber = false
                }

                else if (value.length < 9 || value.length > 18) {
                    newFormErrorMsg.acNumber = "Account Number must be 9-18 characters"
                    newFormValid.acNumber = false
                }
                else {
                    newFormErrorMsg.acNumber = ""
                    newFormValid.acNumber = true
                }
                break;

            case 'mNumber':
                if (value == "") {
                    newFormErrorMsg.mNumber = "Field required"
                    newFormValid.mNumber = false
                }

                else if (value.length != 10) {
                    newFormErrorMsg.mNumber = "Mobile Number should be of 10 digits"
                    newFormValid.mNumber = false
                }
                else {
                    newFormErrorMsg.mNumber = ""
                    newFormValid.mNumber = true
                }
                break;

            case 'TAN':
                if (value == "") {
                    newFormErrorMsg.TAN = "Field required"
                    newFormValid.TAN = false
                }

                else if (!value.match(/^[A-Z]{4}[0-9]{5}[A-Z]{1}$/)) {
                    newFormErrorMsg.TAN = "The TAN should be of format: AAAA12345A"
                    newFormValid.TAN = false
                }
                else {
                    newFormErrorMsg.TAN = ""
                    newFormValid.TAN = true
                }
                break;

            case 'GST':
                if (value == "") {
                    newFormErrorMsg.GST = "Field required"
                    newFormValid.GST = false
                }

                else if (!value.match(/^[0-9]{2}[A-Z]{5}[0-9]{4}[A-Z]{1}[0-9]{1}[A-Z]{1}[0-9]{1}$/)) {
                    newFormErrorMsg.GST = "The GST should be of format:12ABCDE1234A1Z1"
                    newFormValid.GST = false
                }
                else {
                    newFormErrorMsg.GST = ""
                    newFormValid.GST = true
                }
                break;


            case 'password':
                if (value == "") {
                    newFormErrorMsg.password = "Field required"
                    newFormValid.password = false
                }

                else if (!value.match(/^(?=.*[a-z])(?=.*[A-Z])(?=.*\d)(?=.*[@$!%*?&])[A-Za-z\d@$!%*?&]{7,20}$/)) {
                    newFormErrorMsg.password = "Weak Password"
                    newFormValid.password = false
                }
                else {
                    newFormErrorMsg.password = ""
                    newFormValid.password = true
                }
                break;

            default:
                break;

        }
        this.setState(() => ({
            formErrorMessage: newFormErrorMsg,
            formValid: newFormValid,
        }));

        if (newFormValid.name && newFormValid.GST && newFormValid.mNumber && newFormValid.TAN && newFormValid.acNumber && newFormValid.password && newFormValid.email) {
            this.setState((prevState) => ({
                formValid: {
                    ...prevState.formValid,
                    buttonActive: true
                }
            }));
        }
        else {

            this.setState((prevState) => ({
                formValid: {

                    ...prevState.formValid,
                    buttonActive: false
                }
            }));
        }

    }

    submitRegistraion = () => {
        const { email, password, name, TAN, mNumber, GST, acNumber } = this.state.form
        const SellerRegData = { sellerEmail: email, sellerPass: password, sellerName: name, sellerTAN: TAN, sellerGST: GST, sellerPhone: mNumber, sellerAccNo : acNumber  }
        handleSellerRegistration(this.props.dispatch, SellerRegData)
    }

    handleSubmit = event => {
        event.preventDefault();
        this.submitRegistraion();
    }

    componentWillUnmount = () => {
        this.props.dispatch(setMessageAction("",""))
      }

      
    render() {
        const { classes } = this.props;
        const { form, formValid, formErrorMessage } = this.state;

        return (
            <Fragment>
                {(this.props.successMessage != "") ? <SellerLogin /> :
                    (<Container component="main" maxWidth="xs">
                        <CssBaseline />
                        

                        <div className={classes.paper}>

                            <Avatar className={classes.avatar}>
                                <StoreIcon />
                            </Avatar>
                            <Typography component="h1" variant="h5">
                                Seller Registration!
        </Typography></div>

                        <div>
                            <form name = "sellerRegistration" className={classes.form} onSubmit={this.handleSubmit}>

                                <Grid container spacing={2}>
                                    <Grid item xs={12}>
                                        <TextField
                                            autoComplete="fname"
                                            onChange={this.handleChange}
                                            name="name"
                                            variant="outlined"
                                            required
                                            fullWidth
                                            id="name"
                                            label="Name"
                                            value={form.name}
                                            autoFocus
                                        />
                                        <span className="text-danger">{formErrorMessage.name}</span>
                                    </Grid>
                                    <Grid item xs={12}>
                                        <TextField
                                            variant="outlined"
                                            required
                                            fullWidth
                                            id="mNumber"
                                            label="Mobile Number"
                                            type="number"
                                            name="mNumber"
                                            onChange={this.handleChange}
                                            value={form.mNumber}
                                            autoComplete="nNumberd"
                                        />
                                        <span className="text-danger">{formErrorMessage.mNumber}</span>

                                    </Grid>
                                    <Grid item xs={12}>
                                        <TextField
                                            variant="outlined"
                                            required
                                            fullWidth
                                            id="TAN"
                                            label="TAN Number"
                                            type="text"
                                            name="TAN"
                                            onChange={this.handleChange}
                                            value={form.TAN}
                                            autoComplete="TAN"
                                        />
                                        <span className="text-danger">{formErrorMessage.TAN}</span>

                                    </Grid>
                                    <Grid item xs={12}>
                                        <TextField
                                            variant="outlined"
                                            required
                                            fullWidth
                                            id="acNumber"
                                            label="Account Number"
                                            type="text"
                                            name="acNumber"
                                            onChange={this.handleChange}
                                            value={form.acNumber}
                                            autoComplete="acNumber"
                                        />
                                        <span className="text-danger">{formErrorMessage.acNumber}</span>

                                    </Grid>
                                    <Grid item xs={12}>
                                        <TextField
                                            variant="outlined"
                                            required
                                            fullWidth
                                            id="GST"
                                            label="GST Number"
                                            type="text"
                                            name="GST"
                                            onChange={this.handleChange}
                                            value={form.GST}
                                            autoComplete="GST"
                                        />
                                        <span className="text-danger">{formErrorMessage.GST}</span>

                                    </Grid>
                                    <Grid item xs={12}>
                                        <TextField
                                            variant="outlined"
                                            required
                                            fullWidth
                                            id="email"
                                            label="Email Address"
                                            name="email"
                                            type="email"
                                            onChange={this.handleChange}
                                            value={form.email}
                                            autoComplete="email"
                                        />
                                        <span name = "emailError" className="text-danger">{formErrorMessage.email}</span>

                                    </Grid>
                                    <Grid item xs={12}>
                                        <TextField
                                            variant="outlined"
                                            required
                                            fullWidth
                                            name="password"
                                            label="Password"
                                            type="password"
                                            id="password"
                                            onChange={this.handleChange}
                                            value={form.password}
                                            autoComplete="current-password"

                                        />
                                        <span className="text-danger">{formErrorMessage.password}</span>

                                    </Grid>
                                    {/* <Grid item xs={12}>
                                    <FormControlLabel
                                        control={<Checkbox value="allowExtraEmails" color="primary" />}
                                        label="I want to receive inspiration, marketing promotions and updates via email."
                                    />
                                </Grid> */}
                                </Grid>
                                <br />
                                <Button
                                    type="submit"
                                    fullWidth
                                    variant="contained"
                                    color="primary"
                                    name = "submit"
                                    className={classes.submit}
                                    disabled={!formValid.buttonActive}

                                >
                                    Register Seller
                            </Button>
                                <p className="text-danger">{this.props.errorMessage}</p>
                                <p className="text-success">{this.props.successMessage}</p>
                                <Grid container justify="flex-end">
                                    <Grid item>
                                        <Link to="/SellerLogin" variant="body2">
                                            Already have a Seller account? Log in
                                        </Link>
                                        
                                    </Grid>
                                </Grid>
                            </form>
                        </div>
                        <Box mt={5}>
                            <this.Copyright />
                        </Box>
                        
                    </Container>)}
            </Fragment>
        );
    }
}

Register.propTypes = {
    classes: PropTypes.object.isRequired,
};


const mapStateToProps = state => {
    // console.log(state);

    return (
        {
            errorMessage: state.Messages.errorMessage,
            successMessage: state.Messages.successMessage
        })
}

//   export default connect(mapStateToProps)(Register);
export default connect(mapStateToProps)(withStyles(styles)(Register));
