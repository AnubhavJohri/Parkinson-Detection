filename:Cart.js
##SODOC##
import React, { Component } from 'react';
import { connect } from "react-redux";
import { handleEmptyCart, handleUpdateCart, handlePlaceOrder, handleSendCartOnLogout } from '../actions/authedUser'
import { isObjectMember } from '@babel/types';
import { BrowserRouter as Router, Route, Switch ,Redirect} from 'react-router-dom';
import DeleteRoundedIcon from '@material-ui/icons/DeleteRounded';
import DeleteSweepIcon from '@material-ui/icons/DeleteSweep';
import BorderColorIcon from '@material-ui/icons/BorderColor';
import Button from 'react-bootstrap/Button';
import { Card, CardHeader, CardContent } from '@material-ui/core';
import './custom.css'
import { log } from 'util';

export class Cart extends Component {
constructor(props) {
    super(props);
    this.state = {
        data: this.props.authedUser.data[0],
        form:{
            quantity:""
        },
        formValid:{
            quantity:false,
            isbuttonActive:false
        },
        formErrorMessage:{
            quantity:""
        },
        updateQty: false,
        updateId:"",
        successMessage:"",
        errorMessage:"",
        backValid:false,
        totalCartItems:0,
        totalCartPrice:0,
        buyNewValid:false,
        temp:0
    }
}
// "orderId":"P1001",
// "productId":10,
// "productName":"ADIDAS shoes",
// "quantity":1,
// "price":7600,
// "product_img_url":"assets/img/Shoes.jpg"

displayOrderDetails=()=>{
    var cart=this.state.data.cart;
    var total_price=0;
    return (
        <React.Fragment>
             {cart.map((ele,key)=>{
                 var final_price=0;
                 final_price=ele.price-(ele.price*ele.discount/100);       
                total_price=final_price+total_price;     
                //console.log("final_price="+final_price+" total_price="+total_price+"ele.price="+ele.price+"ele.discount="+ele.discount); 
                return(
                    <table className = "table">
                        
                        <tbody>
                            <tr key = {key}>
                                <td><li key={key}>{ele.quantity+"*"+ele.productName+" : "}</li></td>
                                <td className = "text-right" style = {{alignContent : "right", fontWeight : "bold"}}>&#x20b9;{final_price*ele.quantity}</td>
                            </tr>
                        </tbody>
                    </table>
                    // <li key={key}>{ele.quantity+"*"+ele.productName+":"}<b>&#x20b9;{final_price*ele.quantity}</b></li>
                )})}
            <br/>
            <h5 className="display-6">{"Cart Total: "}&#x20b9;{this.state.totalCartPrice}</h5>
        </React.Fragment>
       )
}

onhandleChange = (event) => {
    /* Set emailId state with the value entered into the input field */
    const name = event.target.name;
    const value = event.target.value;
    var newForm=this.state.form;
    newForm[name]=value;
    this.setState({ form: newForm })
    this.validateFields(name,value)
  }

validateFields=(name,value)=>{
    // console.log(name,value);
    var errorMessage=this.state.errorMessage;
    const{formValid,formErrorMessage}=this.state;
    switch(name){
        case 'quantity':
            if(value.length==0){
                formErrorMessage.quantity="field required";
                formValid.quantity=false;
            }
            else if(value<=0){
                formErrorMessage.quantity="Value can't be less than or equal to 0";
                formValid.quantity=false;
            }
            else{
                formErrorMessage.quantity="";
                formValid.quantity=true;
                
            }
        break;
        default:
            break;
    }
    formValid.isbuttonActive=formValid.quantity;
    this.setState({formValid:formValid,formErrorMessage:formErrorMessage})
}


//Deletes all the products in this.state.data[0].cart
//Later on reflects the foloowing changes in redux store
//Have to push changes to DB at the end of the session
emptyCart = () => {
    //console.log("in Cart.js-----------------------------");
    var newUser = this.state.data;
    newUser.cart = []
    this.setState({ data: newUser })
    handleEmptyCart(this.props.dispatch, this.state.data);
}

deleteItem=(productId)=>{
    var newData=this.state.data;
    var cart=this.state.data.cart;
    //console.log("productId=",productId);
    // console.log("my  cart array=",cart,this.state.updateId,cart[0].productId);
    for(let i in cart){
        if(cart[i].productId===productId){
            // console.log("ob=",i,productId,cart[i].productId);
            cart.pop(i);
            break;
        }
    }
    //console.log("my updated cart array=",cart);
    newData.cart=cart;
    //console.log("newData=",newData.cart);
    this.setState({data:newData});
    handleUpdateCart(this.props.dispatch,this.state.data);//This is to update User object in Redux
    // this.setState({successMessage:"Order Quantity Updated!"});    
    this.getTotalCartPrice();
}


//Function to Update The quantity in given OrderId 
onUpdateSubmit=(event)=>{
    event.preventDefault();
    var newData=this.state.data;
    var cart=this.state.data.cart;
    // console.log(cart);
    // console.log("my  cart array=",cart,this.state.updateId,cart[0].productId);
    for(let i in cart){
        if(cart[i].productId===this.state.updateId){
            // console.log("ob=",i,this.state.updateId,cart[i].productId);
            cart[i].quantity=this.state.form.quantity;
        }
    }
    console.log("my updated cart array=",cart);
    newData.cart=cart;
    // console.log("newData",newData.cart);
    this.setState({data:newData});
    handleUpdateCart(this.props.dispatch,this.state.data);//This is to update User object in Redux
    this.setState({successMessage:"Order Quantity Updated!"});
    this.componentDidMount();
    this.getTotalCartPrice();
}

displayUpdateForm=()=>{
    return(
        <div className="container">
                        <div className="row">
                        {/* {this.state.data != undefined && JSON.stringify(this.state.formValid)}
                        {this.state.data != undefined && JSON.stringify(this.state.data.errorMessage)} */}
                            <div className="col-8 offset-3">
                                <form onSubmit={this.onUpdateSubmit}>
                                    <div className="form-group">
                                        <label>Quantity to be added:</label><br />
                                        <input name="quantity" onChange={this.onhandleChange} type="number" className="form-control" placeholder="Enter quantity you want to order" />
                                        <span name="quantityError" className="text-danger">{this.state.formErrorMessage.quantity}</span>
                                    </div>
                                    <button type="submit" name="updateQuantity" disabled={!this.state.formValid.isbuttonActive} className="btn btn-success">Update</button>
                                    <button type="button" name="Back"  className="btn btn-warning" onClick={()=>this.setState({updateQty:false})}>Back</button>
                                    <span name="successMessage" className="text-success">
                                        <br />
                                        <span>{this.state.successMessage}</span>
                                        <span name="errorMessage" className="text-danger">{this.state.errorMessage}</span>
                                    </span>
                                </form>
                               
                            </div>
                        </div>
                    </div>
    )
}

//Displays all cart PRODUCTS IN CARDS
//DYNAMICALLY POPULATES ALL THE CARDS
displayCards = () => {
    var imgclass = { "height": "150px", "width": "150px" }
   
    return (
        // this.state.data != undefined && this.state.data.cart.length != 0 && this.state.data.cart.map((ele, key) => {
        //     return (
                // <React.Fragment>
                //     <div className="col-8 offset-2" key={key} id={key}>
                //         <div className="card bg-light">
                //             <img className="card-img-top" src={ele.product_img_url} alt={ele.productName} />
                //             <div className="card-body">
                //                 <span>{ele.product_img_url}</span>
                //                 <h3 className="card-title">{ele.productName}</h3>
                //                 <p><b>Quantity:</b> {ele.quantity}</p>
                //                 <p><b>Price:</b> {ele.price}</p>
                //                 <button onClick={() => this.setState({ updateQty: true,updateId:ele.productId })} className="btn btn-warning btn-block"><BorderColorIcon/></button>
                //                 <button onClick={() => this.deleteItem(ele.productId)} className="btn btn-danger btn-block"><DeleteRoundedIcon/></button>
                //             </div>
                //         </div>
                //     </div><br />
        <React.Fragment>
            {this.state.data != undefined && this.state.data.cart.length != 0 &&
                <table className="table table-striped">
                    <thead>
                        <tr>
                            <th>Image</th>
                            <th>Name</th>
                            <th>Price</th>
                            <th>Discount</th>
                            <th>Quantity</th>
                            <th>Edit</th>
                            <th>Delete</th>
                        </tr>
                    </thead>
                    <tbody>
                        {
                            this.state.data.cart.map((ele, key) => {
                                // console.log("in element",ele);
                                return (
                                    <React.Fragment>
                                        <tr>
                                            <td><img className="card-img-top imageCard" src={ele.product_img_url} alt={ele.productName} /></td>
                                            <td>{ele.productName}</td>
                                            <td>&#x20b9;{ele.price}</td>
                                            <td>{ele.discount+"%"}</td>
                                            <td>{ele.quantity}</td>
                                            <td><button onClick={() => this.setState({ updateQty: true,updateId:ele.productId })} className="btn btn-block"><BorderColorIcon/></button></td>
                                            <td><button onClick={() => this.deleteItem(ele.productId)} className="btn btn-block"><DeleteRoundedIcon/></button></td>
                                            {/* <td><button onClick={() => this.setState({ updateQty: true, updateOb: ele })} className="btn"><BorderColorIcon /></button></td>
                                            <td><button onClick={() => this.deleteItem(ele.productId)} className="btn"><DeleteRoundedIcon /></button></td> */}
                                        </tr>
                                    </React.Fragment>
                                )
                            })
                        }
                    </tbody>
                </table>
            }
        </React.Fragment>
        //         </React.Fragment>
        //     )
        // })
    )
}

getTotalCartPrice=()=>{
    this.setState({totalCartItems:0})
    var totalPrice=0;
    var finalPrice=0;
    // console.log("i worked");
    var qty=0;
    // if(this.state.data==undefined){
    //     render=()=><Redirect push to="/login"/>
    // }
    if(this.props.authedUser.data.length > 0){
    for(let i in this.state.data.cart){
        finalPrice=this.state.data.cart[i].price-(this.state.data.cart[i].price*this.state.data.cart[i].discount/100);   
        totalPrice=totalPrice+(finalPrice*this.state.data.cart[i].quantity);    
        // console.log("quantity of i=",this.state.data.cart[i].quantity,qty);
        qty=Number(this.state.data.cart[i].quantity)+Number(qty);
    }
    // console.log("quantity=",qty);
    this.setState({totalCartItems:qty,totalCartPrice:totalPrice});
}
}
componentDidMount(){
     this.getTotalCartPrice();
    //  sessionStorage.getItem()
    //  sessionStorage.setItem()
    //  sessionStorage.removeItem()
}

componentWillUnmount(){
    // console.log("cart did unmount");
    if(this.state!=null && this.state.data!=undefined  &&Object.keys(this.state.data).length!=0 ){
        //console.log("cart did unmount",this.state.data.cart);
        var hpAuthedUser=this.state.data
        var stringUserObject=JSON.stringify(hpAuthedUser);
        sessionStorage.setItem("uEmail",stringUserObject);
        handleSendCartOnLogout(this.props.dispatch,this.state.data.cart,this.state.data.userId)
    }
}

placeOrder=()=>{
    // console.log("submitting this object while placing order=",this.state.data);
    handlePlaceOrder(this.props.dispatch,this.state.data).then(res=>{
        alert("Your Order has been Successfully Placed!!")
        this.setState({temp:res});
    })
}

render() {
    //this.componentDidMount();
    //mapStateToProps();
    if(this.state.data==undefined){
        return <Redirect to="/login"/>
    }
    if(this.state.buyNewValid){
        return <Redirect to="/dashboard"/>
    }
    return (
        <React.Fragment>
            {((this.state.data != undefined) && (this.state.data.cart.length != 0) &&(this.state.temp!=1)) ? ((this.state.updateQty === false) ?
                (<div className="container">
                    <div className="row">
                        <div className="col-7  ">
                        <h3 className="text-center">{"Your have " + this.state.data.cart.length + " Items in your cart"}</h3>
                        {this.displayCards()}
                        <Button  variant="warning" size = "block" onClick={() => this.emptyCart(this.state.data.userId)} >Empty Cart <DeleteSweepIcon/></Button>
                    </div>
                    <div className="col-4 offset-1">
                        <Card className="card">
                            <CardHeader className="card-header"  title="Order Details"
        subheader="Checkout" />
                                {/* <h1 className="display-4">Order Details</h1> */}
                            
                            <CardContent>
                                <p>{"Total Items: "+this.state.totalCartItems}</p>
                                <p>{"Cart Items"}</p>
                                <ul>
                                    {this.displayOrderDetails()}
                                </ul>
                            
                            <div className="card-footer">
                                <button className="btn btn-success btn-block" onClick={()=>this.placeOrder()}>Place Order</button>
                            </div>
                            </CardContent>

                    </Card>
                    </div>
                    </div>
                    </div>) : (<div>{this.displayUpdateForm()}</div>)) :
                (
                    <div className="col-8 offset-2 text-center marginClass1">
                        <h3>{"Your Cart is Empty"}</h3>
                        <img className="text-center" alt={"Cart is empty"} src="/assets/cartEmpty.png"></img>
                        <br/>
                        <br/>
                        <br/>
                        <br/>
                        <br/>
                        <button className="btn btn-success" onClick={()=>{this.setState({buyNewValid:true})}} >Click to Buy something!</button>
                    </div>
                )
            }
        </React.Fragment>
    );
}
}
function mapStateToProps({ authedUser }) {
// console.log("in cart",authedUser);
return {
    authedUser,
}
}
export default connect(mapStateToProps)(Cart);
