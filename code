##SODOC##
const he = require( 'hydra-express' );
const Hexpress = he.getExpress();
let router = Hexpress.Router();
const hydra=require('hydra');
const sellerService = require( '../service/seller' );
const user = require( '../model/userData' )

router.post( '/register', ( req, res, next ) => {
    let dat = new user( req.body )
    sellerService.register( dat ).then( result => {
        res.json(result)
    } ).catch( ( err ) => next( err ) )
} )

router.post('/login',(req,res,next)=>{
   return sellerService.sellerLogin(req.body.sellerEmail,req.body.sellerPass).then(data=>{
        res.json({"data":data});
   }).catch(err=>{
       next(err);
   })
})

router.put( '/updateSellerData/:sellerId/:productId', ( req, res, next ) => {
    sellerService.updateSellersData( req.params.sellerId, req.params.productId, req.body ).then( result => {
        if( result ) res.json( { "message": "Seller data updated" } );
    } ).catch( err => next( err ) )
} )

router.get('/deleteProduct/:sellerId/:productId',(req,res,next)=>{
    const productId=req.params.productId;
    const sellerId=req.params.sellerId;
    sellerService.deleteProduct( sellerId,productId ).then( result => {
        if( result ){
            let message=hydra.createUMFMessage({
                to:"products:[POST]/products/deleteSeller",
                from:"seller",
                body:{"productId":productId,
                "sellerId":sellerId}
            })
        
            hydra.makeAPIRequest(message).then(data=>{
               res.json({"data":result});
            })

        } 
    } ).catch( err => next( err ) )
})

router.get('/getSeller/:sellerid',(req,res,next)=>{
    sellerService.getSeller(req.params.sellerid).then(data=>{
        res.json({data})
    }).catch((err) => next(err))
})


router.post('/updatePersonalData/:sellerId',(req,res,next)=>{
    return sellerService.sellerUpdatePersonalData(req.body,req.params.sellerId).then(data=>{
        res.json({"message":data});
    }).catch(err=>{
        next(err);
    })
})

router.post('/addProduct',(req,res,next)=>{
     sellerService.addProduct(req.body).then(data=>{
        res.json({"message":"Product added successfully.","data":data})
    }).catch(err=>{
        next(err);
    })
})

router.post('/checkout',(req,res,next)=>{
    sellerService.checkout(req.body).then(data=>{
        res.json({"sellerMessage":"Seller quantity updated successfully."});
    }).catch((e)=>{
        next(e)
    })
})

module.exports = router;
