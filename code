##SODOC##
const validator = require('../src/utilities/Validator');
const ErrorLogger = require('../src/utilities/errorLogger');
const reqlogger = require('../../src/utilities/requestlogger')  
const req = require('request')

describe('Testing Validator', () => {
    it('1validateEmail: correct Input', () => {
        expect(validator.validateEmail("pallav@example.com")).toEqual(undefined);
    })


    it('2validateEmail: correct Input', () => {
        expect(validator.validateEmail("anubhav@gmail.com")).toEqual(undefined);
    })
    it('3validateEmail: correct Input', () => {
        expect(validator.validateEmail("tushar@tushar.com")).toEqual(undefined);
    })
    it('4validateEmail: correct Input', () => {
        expect(validator.validateEmail("keshav@yahoo.com")).toEqual(undefined);
    })
    it('5validateEmail: correct Input', () => {
        expect(validator.validateEmail("john@johnson.com")).toEqual(undefined);
    })

    it('6validateEmail: incorrect Input', () => {
        expect(function () { validator.validateEmail("Kids101@gmail.com") }).toThrow(new Error("Wrong Email"));
    })

    it('7validateEmail: incorrect Input', () => {
        expect(function () { validator.validateEmail("Tushar.gmail.com") }).toThrow(new Error("Wrong Email"));
    })


    it('8validatePassword: correct Input', () => {
        expect(validator.validatePassword("John123@")).toEqual(undefined);
    })
    it('9validatePassword: correct Input', () => {
        expect(validator.validatePassword("Johnson@123")).toEqual(undefined);
    })
    it('10validatePassword: correct Input', () => {
        expect(validator.validatePassword("jame5Bond@007")).toEqual(undefined);
    })
    it('11validatePassword: correct Input', () => {
        expect(validator.validatePassword("Hitler@420demon")).toEqual(undefined);
    })
    it('12validatePassword: incorrect Input', () => {
        expect(function () { validator.validatePassword("hitler@12") }).toThrow(new Error("Password not strong"));
    })
    it('13validatePassword: incorrect Input', () => {
        expect(function () { validator.validatePassword("jamesBond007") }).toThrow(new Error("Password not strong"));
    })

    it('14validatePassword: incorrect Input', () => {
        expect(function () { validator.validatePassword("narendraModi") }).toThrow(new Error("Password not strong"));
    })
})

describe('Testing ErrorLogger', () => {
    let obj = { "errorLogger": ErrorLogger }
    let next = () => { }
    beforeEach(() => {
        res = {
            status: undefined,
            json: undefined,
            status: function (status) {
                this.status = status
            },
            json: function (val) {
                this.json = val
            }
        }
    })

    it('Response status set to 500 by default for Error', () => {
        let err = new Error("ErrorLogger test");
        try {
            obj.errorLogger(err, req, res, next)
        } catch (e) { }
        finally {
            expect(res.status).toEqual(500);
        }
    })


    it('Response status is set using Error status', () => {
        let err = new Error("ErrorLogger test");
        err.status = 404
        try {
            obj.errorLogger(err, req, res, next)
        } catch (e) { }
        finally {
            expect(res.status).toEqual(404);
        }
    })

    it('Response Should Contain Error Message', () => {
        let err = new Error("ErrorLogger test");
        try {
            obj.errorLogger(err, req, res, next)
        } catch (e) { }
        finally {
            expect(res.json.message).toEqual(err.message);
        }
    })
})

describe("Testing RequestLogger", () => {
	let obj = { "Requestlogger": reqlogger }
	let nextObj;
	let res = {}
	beforeEach(() => {
		nextObj = { next() { } }
		spyOn(nextObj, 'next')

	})
	it('next should be called', () => {
		try {
			obj.Requestlogger(req, res, nextObj.next);
		}
		finally {
			expect(nextObj.next).toHaveBeenCalled();
		}
	})
});
