const dbModel = require('../model/seller')
const validator=require('../utilities/validator');
let seller = {}

seller.register = (sellerDetails) => {
    validator.checkAccNo(sellerDetails.sellerAccNo);
    validator.checkTAN(sellerDetails.sellerTAN);
    validator.checkGST(sellerDetails.sellerGST);
    return dbModel.register(sellerDetails).then(result => {
        if (result) return result
        let err = new Error("Seller register failed in service");
        err.status = 404;
        throw err;
    } )
}
seller.sellerLogin=( sellerEmail,sellerPassword )=>{
    return dbModel.sellerLogin( sellerEmail,sellerPassword ).then( data=>{
        if( data ){
            return data;
        }
    } ).catch(e=>{throw e});
}

seller.updateSellersData = ( sellerId, productId, obj ) => {
    validator.checkSId(sellerId);
    return dbModel.updateSellersData( sellerId, productId, obj ).then( res => {
        return res
    } )
}
seller.deleteProduct = ( sellerId, productId ) => {
    validator.checkSId(sellerId);
     return dbModel.deleteProduct( sellerId, productId).then( res => {
         return res
     } )
 }

seller.getSeller=( sellerid )=>{
    validator.checkSId(sellerid);
    return dbModel.getSeller(sellerid).then(data=>{
            return data
       } ).catch((e)=>{throw e;})
}


seller.sellerUpdatePersonalData=(data,sellerId)=>{
    validator.checkSId(sellerId);
    return dbModel.sellerUpdatePersonalData(data,sellerId).then(data=>{
        if(data){
            return data;
        }
    })
}

seller.addProduct=(product)=>{
    return dbModel.addProduct(product).then(data=>{
        return data;
    })

}
seller.checkout=(product)=>{
    return dbModel.checkout(product).then(data=>{
        return data;
      })
}

module.exports = seller;
