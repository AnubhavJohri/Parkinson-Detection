##SODOC##
import React, { Component, Fragment } from 'react';

import Avatar from '@material-ui/core/Avatar';
import Button from '@material-ui/core/Button';
import CssBaseline from '@material-ui/core/CssBaseline';
import TextField from '@material-ui/core/TextField';
import FormControlLabel from '@material-ui/core/FormControlLabel';
import Checkbox from '@material-ui/core/Checkbox';
// import Link from '@material-ui/core/Link';
import FormControl from '@material-ui/core/FormControl';
// import ColorPicker from 'material-ui-color-picker'
import Grid from '@material-ui/core/Grid';
import Paper from '@material-ui/core/Paper';
import Select from '@material-ui/core/Select'
import MenuItem from '@material-ui/core/MenuItem'
import CloudUploadIcon from '@material-ui/icons/CloudUpload';
import InputLabel from '@material-ui/core/InputLabel'
import Box from '@material-ui/core/Box';
import LockOutlinedIcon from '@material-ui/icons/LockOutlined';
import Typography from '@material-ui/core/Typography';
import { makeStyles } from '@material-ui/core/styles';
import Container from '@material-ui/core/Container';
import { handleAddProduct } from '../actions/authedSeller';
import { connect } from 'react-redux'
import PropTypes from 'prop-types';
import withStyles from '@material-ui/core/styles/withStyles';
import Login from './Login';
import { Link } from 'react-router-dom'
import { BrowserRouter as Router, Route, Switch ,Redirect} from 'react-router-dom';
import FileBase64 from 'react-file-base64';





const styles = theme => ({
    '@global': {
        body: {
            backgroundColor: theme.palette.common.white,
        },
    },
    paper: {
        marginTop: theme.spacing(8),
        display: 'flex',
        flexDirection: 'column',
        alignItems: 'center',
    },
    avatar: {
        margin: theme.spacing(1),
        backgroundColor: theme.palette.secondary.main,
    },
    form: {
        width: '100%', // Fix IE 11 issue.
        marginTop: theme.spacing(3),
    },
    submit: {
        margin: theme.spacing(1),
    },
    formControl: {
        margin: theme.spacing(1),
        minWidth: '100%'
    },
    rightIcon: {
        marginLeft: theme.spacing(1),
      },
});


class AddProducts extends Component {
    constructor(props) {
        super(props);
        this.state = {
            form: {
                productCategory: "",
                productName: "",
                productManufacturer: "",
                productDescription: "",
                productPrice: "",
                productDiscount: "",
                productQuantity: "",
                productImage:"",
                Size: "",
                Color: "",
                RAM: "",
                Memory: "",
                Material: "",
                files: ""
       
        }
    }
}

getFiles(files){
    // console.log("files=",files[0].base64);
    this.setState({ files: files[0].base64 })
  }

    Copyright = () => {
        return (
            <Typography variant="body2" color="textSecondary" align="center">
                {'Copyright © '}
                <Link color="inherit" href="/dashboard">
                    Hoopla
      </Link>{' '}
                {new Date().getFullYear()}
                {'.'}
            </Typography>
        );
    }


handleChange = (event) => {
    const value = event.target.value;
    const name = event.target.name;
    // console.log(name,value);    
    this.setState({ form: { ...this.state.form, [name]: value } })
    // console.log("state=",this.state);   
    
}

    submitRegistraion = () => {
        const sellerId = this.props.authedSeller.data[0].sellerId;
        const sellerName = this.props.authedSeller.data[0].sellerName;
        const {files,productImage, productCategory, productName, productManufacturer, productDescription, productPrice, productDiscount, productQuantity, Size, Color, Material, RAM, Memory } = this.state.form;
        
        const productData = {
            productName: productName.toLowerCase(),
            productCategory: productCategory,
            productManufacturer: productManufacturer.toLowerCase(),
            productDescription: productDescription,
            productDiscount: productDiscount,
            productPrice: productPrice,
            productQuantity: productQuantity,
            productImage:this.state.files,
            s_id: sellerId,
            s_name: sellerName, 
            productSpecs : {
                Size,
                Color,
                Material,
                RAM,
                Memory
            } }
            // console.log("data being sent=",productData);                   
           handleAddProduct(this.props.dispatch, productData)
    }

    handleSubmit = event => {
        event.preventDefault();
        this.submitRegistraion();
    }



    render() {
        const { classes } = this.props;
        const { productCategory, productName, productManufacturer, productDescription, productPrice, productDiscount, productQuantity, Size, Color, Material, RAM, Memory } = this.state.form;
        //console.log(this.props.authedSeller);
        
       if(this.props.authedSeller.data.length==0){
           return <Redirect push to="/SellerLogin"/>
       }
        return (
            <Fragment>
                {JSON.stringify(this.state.productImage)}
                
                    <Container component="main" maxWidth="xs">
                        <CssBaseline />
                        <div className={classes.paper}>

                            <Avatar className={classes.avatar}>
                                <LockOutlinedIcon />
                            </Avatar>
                            <Typography component="h1" variant="h5">
                                Fill the form to add new Products!
        </Typography></div>

                        <div>
                            <form className={classes.form} onSubmit={this.handleSubmit}>

                                <Grid container spacing={2}>
                                    <Grid item xs={12}>
                                        <TextField
                                            autoComplete="productName"
                                            onChange={this.handleChange}
                                            name="productName"
                                            variant="outlined"
                                            required
                                            fullWidth
                                            id="productName"
                                            label="Product Name"
                                            placeholder="Black Cotton Shirt"
                                            value={productName}
                                            autoFocus
                                        />

                                    </Grid>
                                    <Grid item xs={12}>
                                        <TextField
                                            variant="outlined"
                                            required
                                            fullWidth
                                            id="productManufacturer"
                                            label="Product Manufacturer"
                                            type="text"
                                            name="productManufacturer"
                                            placeholder="Samsung, PepperFry, Adidas, VanHueSen"
                                            onChange={this.handleChange}
                                            value={productManufacturer}
                                            autoComplete="productManufacturer"
                                        />


                                    </Grid>
                                    {/* <Grid item xs={12}>
                                        <input
                                            variant="outlined"
                                            required
                                            fullWidth
                                            id="productManufacturer"
                                            label="Product Image"
                                            type="file"
                                            name="productImage"
                                            onChange={this.handleChange}
                                            // value={productManufacturer}
                                            autoComplete="productManufacturer"
                                        />
                                    </Grid> */}
                                    <FileBase64
                                        multiple={ true }
                                        onDone={ this.getFiles.bind(this) } />


                                    <Grid item xs={12}>
                                        <TextField
                                            variant="outlined"
                                            required
                                            fullWidth
                                            id="productDescription"
                                            label="Product Description"
                                            type="text"
                                            name="productDescription"
                                            placeholder="Solid black cotton shirt, hand-stiched....."
                                            onChange={this.handleChange}
                                            value={productDescription}
                                            autoComplete="productDescription"
                                        />


                                    </Grid>
                                    <Grid item xs={12}>
                                        <TextField
                                            variant="outlined"
                                            required
                                            fullWidth
                                            id="productPrice"
                                            label="Product Price &#x20b9;"
                                            type="number"
                                            name="productPrice"
                                            placeholder="&#x20b9; 8500"
                                            onChange={this.handleChange}
                                            value={productPrice}
                                            autoComplete="productPrice"
                                        />

                                    </Grid>
                                    <Grid item xs={12}>
                                        <TextField
                                            variant="outlined"
                                            required
                                            fullWidth
                                            id="productDiscount"
                                            label="Product Discount"
                                            name="productDiscount"
                                            type="number"
                                            placeholder = "10%"
                                            onChange={this.handleChange}
                                            value={productDiscount}
                                            autoComplete="productDiscount"
                                        />


                                    </Grid>
                                    <Grid item xs={12}>
                                        <TextField
                                            variant="outlined"
                                            required
                                            fullWidth
                                            name="productQuantity"
                                            label="Product Quantity"
                                            type="number"
                                            placeholder = "6"
                                            id="productQuantity"
                                            onChange={this.handleChange}
                                            value={productQuantity}
                                            autoComplete="productQuantity"

                                        /> <br />
                                        <Grid item xs={12}>
                                            <FormControl className={classes.formControl}>
                                                <InputLabel htmlFor="productCategory">Product Category</InputLabel>
                                                <Select
                                                    value={productCategory}
                                                    onChange={this.handleChange}
                                                    name='productCategory'
                                                    id='productCategory'

                                                >
                                                    <MenuItem value="Electronics">Electronics</MenuItem>
                                                    <MenuItem value="Shoes">Shoes</MenuItem>
                                                    <MenuItem value="Furniture">Furniture</MenuItem>
                                                    <MenuItem value="Clothing">Clothing</MenuItem>
                                                </Select>
                                            </FormControl>
                                        </Grid>

                                        {(productCategory == "") ? (
                                            <div> </div>
                                        ) : (
                                                (productCategory == "Electronics") ? (
                                                    <div>
                                                        <Grid item xs={12}>
                                                            <FormControl className={classes.formControl}>
                                                                <InputLabel htmlFor="RAM">RAM</InputLabel>
                                                                <Select
                                                                    value={RAM}
                                                                    onChange={this.handleChange}
                                                                    name='RAM'
                                                                    id='RAM'

                                                                >
                                                                    <MenuItem value="2 GB">2 GB</MenuItem>
                                                                    <MenuItem value="4 GB">4 GB</MenuItem>
                                                                    <MenuItem value="6 GB">6 GB</MenuItem>
                                                                    <MenuItem value="8 GB">8 GB</MenuItem>
                                                                </Select>
                                                            </FormControl>
                                                        </Grid>
                                                        <Grid item xs={12}>
                                                            <FormControl className={classes.formControl}>
                                                                <InputLabel htmlFor="Memory">Internal Storage</InputLabel>
                                                                <Select
                                                                    value={Memory}
                                                                    onChange={this.handleChange}
                                                                    name='Memory'
                                                                    id='Memory'

                                                                >
                                                                    <MenuItem value="16 GB">16 GB</MenuItem>
                                                                    <MenuItem value="64 GB">64 GB</MenuItem>
                                                                    <MenuItem value="128 GB">128 GB</MenuItem>
                                                                    <MenuItem value="256 GB">256 GB</MenuItem>
                                                                </Select>
                                                            </FormControl>
                                                        </Grid>
                                                    </div>
                                                ) : (
                                                        (productCategory == "Shoes" || productCategory == "Clothing") ? (
                                                            <div>
                                                                <Grid item xs={12}>
                                                                    <FormControl className={classes.formControl}>
                                                                        <InputLabel htmlFor="Size">Size</InputLabel>
                                                                        <Select
                                                                            value={Size}
                                                                            onChange={this.handleChange}
                                                                            name='Size'
                                                                            id='Size'

                                                                        >
                                                                            <MenuItem value="7">UK 7</MenuItem>
                                                                            <MenuItem value="8">UK 8</MenuItem>
                                                                            <MenuItem value="9">UK 9</MenuItem>
                                                                            <MenuItem value="10">UK 10</MenuItem>
                                                                        </Select>
                                                                    </FormControl>
                                                                </Grid>
                                                                <Grid item xs={12}>
                                                                    <FormControl className={classes.formControl}>
                                                                        <InputLabel htmlFor="Color">Color</InputLabel>
                                                                        <Select
                                                                            value={Color}
                                                                            onChange={this.handleChange}
                                                                            name='Color'
                                                                            id='Color'

                                                                        >
                                                                            <MenuItem value="black">Black</MenuItem>
                                                                            <MenuItem value="blue">Blue</MenuItem>
                                                                            <MenuItem value="red">Red</MenuItem>
                                                                            <MenuItem value="green">Green</MenuItem>
                                                                        </Select>
                                                                    </FormControl>
                                                                </Grid>
                                                            </div>
                                                        ) : ((productCategory == "Furniture") ? (
                                                            <div>
                                                                <Grid item xs={12}>
                                                                    <FormControl className={classes.formControl}>
                                                                        <InputLabel htmlFor="Material">Material</InputLabel>
                                                                        <Select
                                                                            value={Material}
                                                                            onChange={this.handleChange}
                                                                            name='Material'
                                                                            id='Material'

                                                                        >
                                                                            <MenuItem value="Wooden">Wooden</MenuItem>
                                                                            <MenuItem value="Iron">Iron</MenuItem>
                                                                            <MenuItem value="Steel">Steel</MenuItem>
                                                                            
                                                                        </Select>
                                                                    </FormControl>
                                                                </Grid>
                                                            </div>
                                                        ) : (null)
                                                        )
                                                    )
                                            )}
                                    </Grid>
                                    {/* <Grid item xs={12}>
                                    <FormControlLabel
                                        control={<Checkbox value="allowExtraEmails" color="primary" />}
                                        label="I want to receive inspiration, marketing promotions and updates via email."
                                    />
                                </Grid> */}
                                </Grid>
                                <br />
                                <Button
                                    type="submit"
                                    fullWidth
                                    variant="contained"
                                    color="primary"
                                    className={classes.submit}
                                // disabled={!formValid.buttonActive}
                                >
                                    Add Product
                                    <CloudUploadIcon className={classes.rightIcon} />
                            </Button>
                                <p className="text-danger">{this.props.errorMessage}</p>
                                <p className="text-success">{this.props.successMessage}</p>

                            </form>
                        </div>
                        <Grid container justify="flex-end">
                            <Grid item>
                                <Link to="/SellerProducts" exact={"true"} >Navigate to View Products</Link><br ></br>
                            </Grid>
                        </Grid>
                        <Box mt={5}>
                            <this.Copyright />
                        </Box>

                    </Container>
            </Fragment>
        );
    }
}

AddProducts.propTypes = {
    classes: PropTypes.object.isRequired,
};


const mapStateToProps = state => {
    // console.log(state);

    return (
        {
            authedSeller:state.authedSeller,
            errorMessage: state.Messages.errorMessage,
            successMessage: state.Messages.successMessage
        })
}

//   export default connect(mapStateToProps)(AddProducts);
export default connect(mapStateToProps)(withStyles(styles)(AddProducts));























