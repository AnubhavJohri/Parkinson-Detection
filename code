#############################################################################################################################
filepath:Project\hoopla_seller-master\src\model\seller.js##EOFP##
filename:seller.js
##SODOC##
const dbLayer = require( '../utilities/connection' )

let seller = {}

seller.generateId = () => {
    return dbLayer.getCollection().then( result => {
        return result.aggregate( [{ $project: { "sellerId": 1, _id: 0 } },
        { $sort: { "sellerId": -1 } }, { $limit: 1 }] ).then( ( id ) => {
           
            let maxId = id[0].sellerId;
            let max = parseInt( maxId.substring( 1, 5 ) ) + 1;
           
            return"S" + max;
        } )
    } )
}

seller.register = ( sellerDetails ) => {
    return dbLayer.getCollection().then( data => {
        return seller.generateId().then( id => {
            sellerDetails.sellerId = id
            return data.findOne({ sellerEmail: sellerDetails.sellerEmail }).then(result => {
                if (!result || result.length == 0) {
                    return data.findOne({ sellerAccNo: sellerDetails.sellerAccNo }).then(result1=>{
                        if (!result1 || result1.length == 0) {
                            return data.findOne({ sellerTAN: sellerDetails.sellerTAN }).then(result2=>{
                                if (!result2 || result2.length == 0) {
                                    return data.findOne({ sellerGST: sellerDetails.sellerGST }).then(result3=>{
                                        if (!result3 || result3.length == 0) {
                                            return data.create(sellerDetails).then(iresult => {
                                                if (iresult.sellerName == sellerDetails.sellerName) {
                                                    return {"data":iresult}
                                                }
                                                else{
                                                    return { "message":"Insertion of seller data failed."}
                        
                                                }
                                            } )
                
                                        }
                                        else{
                                            let err = new Error("This GST number already exists.");
                                            err.status = 409;
                                            throw err;
                                        }
                                    })
        
                                }
                                else{
                                    let err = new Error("This TAN number already exists.");
                                            err.status = 409;
                                            throw err;
                                }
                            })
                            

                        }
                        else{
                            let err = new Error("This Account number already exists.");
                                            err.status = 409;
                                            throw err;
                        }
                    })
                    
                }
                else{

                    let err = new Error("This email id is already registered.");
                    err.status = 409;
                    throw err;
                }

            } )
        } )

    } )
}   

seller.sellerLogin=( sellerEmail,sellerPassword )=>{
    return dbLayer.getCollection().then( model=>{
        return model.find( {sellerEmail: sellerEmail} ).then( data=>{
           
            if( data.length>=1 && data!=[] ){
                if( data[0]['sellerPass']===sellerPassword ){
                    return data[0];
                }
                else{
                    let err=new Error("Check email or password again");
                    throw err;
                }
            }
            else{
                let err=new Error("Please Register to login");
                throw err;
            }
        } )
    } )
}

seller.updateSellersData = ( sellerId, productId, obj ) => {
   
    const {productQuantity,price,discount}=obj;
    return  dbLayer.getCollection().then( data => {
        
        if(productQuantity){
        return data.updateOne( { "sellerId": sellerId, "sellerProducts.productId": productId },
        {$set: {"sellerProducts.$.productQuantity": productQuantity} } ).then( res => {
            if( res.nModified >= 1 ){
                if(price){
                    return data.updateOne( { "sellerId": sellerId, "sellerProducts.productId": productId },
                    {$set: {"sellerProducts.$.price": price} } ).then( res => {
                        if( res.nModified >= 1 ){
                            if(discount){
                                return data.updateOne( { "sellerId": sellerId, "sellerProducts.productId": productId },
                                {$set: {"sellerProducts.$.discount": discount} } ).then( res => {
                                    if( res.nModified >= 1 ){
                                        return true
                                    }
                                    else{
                                        let err = new Error( "discount update failed" );
                                        err.status = 404;
                                        throw err;
                                    }
                                } )
                            }
                        
                            return true
                        }
                        else{
                            let err = new Error( "price update failed" );
                            err.status = 404;
                            throw err;
                        }
                    } )
                }
                return true
            }
            else{
                let err = new Error( "quantity update failed" );
                err.status = 404;
                throw err;
            }
        } )
    }
    

    } )
}

seller.deleteProduct=(sellerId,productId)=>{
    return dbLayer.getCollection().then(model=>{
        return model.updateOne({sellerId:sellerId},{$pull:{"sellerProducts":{"productId":productId}}}).then(data=>{
            
            if(data.nModified>0){
               return model.find({sellerId:sellerId},{_id:0}).then(sel=>{
                   return sel;
               })
               
            }else{
                return null;
            }
        })
    })
}

seller.getSeller = ( sellerId ) => {
    return dbLayer.getCollection().then( data => {
        return data.findOne( { sellerId: sellerId } ).then( data => {
            if(data){
                return data;}
            else{
         
                let error=new Error("Invalid seller id.")
                throw error;
            }    
            
            
        } )
            
        })
    } 


seller.sellerUpdatePersonalData=(data,sellerId)=>{
    return dbLayer.getCollection().then(model=>{
        return model.updateOne({sellerId:sellerId},{$set:{sellerName:data.sellerName,sellerEmail:data.sellerEmail,
        sellerPhone:data.sellerPhone,sellerPass:data.sellerPass,sellerAccNo:data.sellerAccNo,
        sellerTAN:data.sellerTAN,sellerGST:data.sellerGST}}).then(data=>{
            if(data.nModified>=1){
                return data;
            }
            else{
                let err=new Error("Seller data could not be updated");
                err.status=500;
                
            }
        })
    })
}

seller.addProduct=(product)=>{
    
    let obj={
        "productId": product.productId,
        "productQuantity": product.sellerDetails[0].productQuantity,
        "price":product.sellerDetails[0].productPrice,
        "discount":product.sellerDetails[0].productDiscount,
        "productName": product.productName,
    }

    return dbLayer.getCollection().then(model=>{
        return model.updateOne({sellerId:product.sellerDetails[0].s_id},{$push:{sellerProducts:obj}}).then(data=>{
            if(data.nModified>=1){
                return model.find({sellerId:product.sellerDetails[0].s_id},{_id:0}).then(sel=>{
                    return sel;
                })
            }
            else{
                let err=new Error("Product was not added in sellerDB.");
                err.status=500;
                
            }
        })
    })

}
seller.checkout=(product)=>{

    return dbLayer.getCollection().then(model=>{
        let i=0;
       
        return model.updateOne({$and:[{sellerId:product.product[i].sellerDetails.s_id},{"sellerProducts.productId":product.product[i].productId}]},{$inc:{"sellerProducts.$.productQuantity":-product.quantity}}).then(data=>{
           
               return model.aggregate([{$unwind:"$sellerProducts"},{$match:{$and:[{sellerId:product.product[i].sellerDetails.s_id},{"sellerProducts.productId":product.product[i].productId}]}},{$project:{_id:0,"sellerProducts":1,sellerId:1}}]).then(sid=>{
                  if(sid){
               
                      return sid;
                  }
              })
        })
    })
}


module.exports = seller
##EODOC##



