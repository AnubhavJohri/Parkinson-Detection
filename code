filepath:Project\hoopla_uinew1-master\src\actions\authedUser.js##EOFP##
filename:authedUser.js
##SODOC##
import { placeOrder,getSellersOfProducts, hpLogin, submitRegistrationAPI, emptyCartAPI, searchProducts, deleteSellerProductAPI, sendCartOnLogout } from '../utils/api'
import { setHPAuthedSeller } from './authedSeller';

export const SET_HP_AUTHED_USER = 'SET_HP_AUTHED_USER';
export const SET_MESSAGE = 'SET_MESSAGE';
export const SET_HP_ALL_PRODUCTS = 'SET_HP_ALL_PRODUCTS';


export function setSelectedProduct (allProducts,isError) {
  // console.log("allproducts in action creator=",allProducts);  
  return {
    type: SET_HP_ALL_PRODUCTS,
    allProducts,
    isError,
  }
}



export function setHPAllProducts (allProducts,isError) {
  // console.log("allproducts in action creator=",allProducts);  
  return {
    type: SET_HP_ALL_PRODUCTS,
    allProducts,
    isError,
  }
}

export function setHPAuthedUser (hpAuthedUser,isError) {
  return {
    type: SET_HP_AUTHED_USER,
    hpAuthedUser,
    isError,
  }
}



export const setMessageAction = (successMessage, errorMessage) => {
  /* your code goes here */
  // console.log(errorMessage)
  return({
      type : SET_MESSAGE,
      successMessage,
      errorMessage
  })
}


//Just to add items in crate
export function handleSetHPAuthedUserCrate(dispatch,hpAuthedUser){
  // console.log("in handleSetHPAuthedUserCrate",hpAuthedUser);
  dispatch(setHPAuthedUser(hpAuthedUser,""))
}

export function handleSetHPAuthedUser(dispatch, loginData) { 
  return hpLogin(loginData).then( (hpAuthedUser) => {   
        dispatch(setHPAuthedUser(hpAuthedUser,null))
        return hpAuthedUser;
      })
    .catch(err => {
      
      dispatch(setHPAuthedUser(null, err.message))
    })
}

export const handleSubmitRegistration = (dispatch, formData) => {
  return submitRegistrationAPI(formData).then(smsg => {
      
      dispatch(setMessageAction(smsg.message, ""))
  }).catch(err => {
   dispatch(setMessageAction("", err.message))})
}

export const handleEmptyCart = (dispatch,hpAuthedUser) => {
  dispatch(setHPAuthedUser(hpAuthedUser,null))
}

export const handleUpdateCart = (dispatch,sellerId,productId) => {
  deleteSellerProductAPI(sellerId,productId).then(res=>{
    // console.log("----------",res);
    dispatch(setHPAuthedSeller(res,""));
    dispatch(setMessageAction("Successfully Deleted!",""))
  }).catch(err=>{
    dispatch(setHPAuthedSeller([],""));
    dispatch(setMessageAction("",err.message));
  })
}


export const handleSearchCategory=(dispatch, search)=>
{
  // console.log(typeof dispatch);
  return searchProducts(search).then(result => {
    // console.log(result.data);
    
    dispatch(setHPAllProducts(result.data, ""))
}).catch(err => {
 dispatch(setHPAllProducts([],err.message))
})
}


export const handleGetSeller=(dispatch,productId)=>{
  return getSellersOfProducts(productId).then(res=>{
    return res;
  }).catch(err=>{
    dispatch(setMessageAction("",err.message));
  });
}

export const handlePlaceOrder=(dispatch,userObj)=>{
 return placeOrder(userObj).then(data=>{
    // console.log("in authedUser cart length being dispatched=",data.cart.length);
    dispatch(setHPAuthedUser(data,""));
    dispatch(setMessageAction("Successfully Placed Your Order",""));
    return 1;
    // var newObj=userObj;
    // for(let i in newObj.cart){
    //   newObj.orders.push(newObj.cart[i]);
    // }
    // newObj.cart=[];
  }).catch(err=>{
    dispatch(setMessageAction("",err.message));
    dispatch(setHPAuthedUser(userObj,err.message));
  });
}
export const handleSendCartOnLogout=(dispatch,cart,userId)=>{
  return sendCartOnLogout(cart,userId).then(res=>{
    // console.log(res);
    //setMessageAction("","");
  })
}

//Created just to set errors and success messages
export const handleSetErrors=(dispatch,successMsg,errorMsg)=>{
  dispatch(setMessageAction(successMsg,errorMsg));

}




//setHPAllProducts([],err.message)
