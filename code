##SODOC##
const dbLayer = require( '../model/user' );

let user = {}

//Verfying the credentials of user
user.loginUser = ( uEmail, pass ) => {
    return dbLayer.userLogin( uEmail, pass ).then( response => {
        return response;
    } )
}

user.addOrder = ( userId, successData ) => {
    return dbLayer.addOrder( userId, successData ).then( response => {
        return response;
    } )
}


user.clearCart = ( userId ) => {
    return dbLayer.clearCart( userId ).then( response => {
        if( response )
        return response;
        else{
            let err = new Error( "Internal Server Error. Access your cart after sometime." );
            err.status = 401;
            throw err;
        }
    } )
}

user.checkout=( userDetails )=>{
    return dbLayer.checkout( userDetails ).then( response=>{
        if( response ){
      //  console.log("Service response : ",response)
        return response;}
        else{
            let err = new Error( "Internal Server Error. Try again later." );
            err.status = 401;
            throw err;
        }
    } )
}
user.registerUser = ( Person ) => {
    return dbLayer.userRegister( Person ).then( response => {
        return response;
    } )
}
user.updateCart = (userid,cart) => {
    return dbLayer.updateCart(userid,cart).then( response => {
        return response;
    }).catch((e)=>{throw e})
}
user.add = ( userid, product ) => {
    return dbLayer.add( userid, product ).then( response => {
        return response;
    } )
}
user.cartDetails = ( userid ) => {
    return dbLayer.cartDetails( userid ).then( response => {
        return response;
    } )
}
user.deleteproduct = ( userid, productid ) => {
    return dbLayer.deleteproduct( userid, productid ).then( response => {
        return response;
    } )
}
user.getPastOrders = ( userId ) => {
    // console.log( 'atrayee service' );
    return dbLayer.getPastOrders( userId ).then( response => {
        return response;
    } )
}
user.userdata = ( userid ) => {
    return dbLayer.userdata( userid ).then( response => {
        return response;
    } )
}
user.updateuser = ( userid, user ) => {
    //console.log("Middle",user)
    return dbLayer.updateuser( userid, user ).then( response => {
        return response;
    } )
}
user.updatepassword = ( userid, user ) => {
    return dbLayer.updatepassword( userid, user ).then( response => {
        return response;
    } )
}
module.exports = user
##EODOC##



------EOD------
#############################################################################################################################
filepath:Project\hoopla_user-master\src\utilities\connection.js##EOFP##
filename:connection.js
##SODOC##
const{ Schema } = require( 'mongoose' );
const mongoose = require( 'mongoose' );
mongoose.Promise = global.Promise;
mongoose.set( 'useCreateIndex', true );
const url = "mongodb://localhost:27017/userDB"

const usersSchema = Schema( {
    userId: { type: String, required: [true, 'userId is required'] },
    address: { type: String },
    uCredentials: {
        uEmail: { type: String, required: [true, 'uEmail is required'], unique: true },
        uPass: { type: String, required: [true, 'uPass is required'] }
    },
    uProfile: {
        uName: { type: String, required: [true, 'uName is required'] },
        uDOB: { type: Date, required: [true, 'uDOB is required'] },
        uPhone: { type: Number, required: [true, 'uPhone is required'] },
        uAddress: { type: String },
        uIsSeller: { type: Boolean, default: false },
        uDateJoined: { type: Date, default: new Date().toISOString() },
        uLastLogin: { type: Date, default: new Date().toISOString() }

    },
    cart: [{
        productId: {type: String},
        productName: {type: String},
        quantity: {type: Number},
        price: {type: Number},
        product_img_url: {type: String},
        sellerName: {type: String},
        discount: {type: Number}
    }],
    orders: [{
        orderId: { type: Number },
        productId: { type: String },
        productName: { type: String },
        quantity: { type: Number },
        price: { type: Number },
        discount: {type: Number},
        product_img_url: { type: String },
        orderTime: {type: Date, default: new Date()}
    }]
}, { collection: "User", timestamps: true } )

let connection = {}

//Returns model object of "Users" collection
connection.getCollection = () => {
    //Establish connection and return model as promise
    return mongoose.connect( url, { useNewUrlParser: true } ).then( database => {
        return database.model( 'User', usersSchema )
    } ).catch( error => {
        // console.log( error );
        let err = new Error( "Could not connect to the database" );
        err.status = 500;
        throw err;
    } );
}

module.exports = connection;
##EODOC##



