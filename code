class userDetails {
    constructor(obj) {
    this.sellerId= 0,
    this.sellerName=obj.sellerName,
    this.sellerEmail= obj.sellerEmail,
    this.sellerPhone= obj.sellerPhone,
    this.sellerPass= obj.sellerPass,
    this.sellerAccNo=obj.sellerAccNo,
    this.sellerTAN=obj.sellerTAN,
    this.sellerGST= obj.sellerGST,
    this.sellerProducts=[]
    }
}

module.exports = userDetails;
