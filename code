##SODOC##
import axios from 'axios';
import Axios from 'axios';
const usersBackendURL = "http://localhost:1111/user";
const productsBackendURL = "http://localhost:1111/products";
const sellerBackendURL = "http://localhost:1111/seller";


//USER API calls                                              
//To login the user
 export function hpLogin(loginData) {
   return axios.post(usersBackendURL+'/login',loginData)
   .then( response =>  response.data.data[0] )
   .catch(error =>{
      
          let err = new Error();
    if (error.response) {
      err.message = error.response.data.message;
    } else {
      err.message = 'Server Error, Please try later';
    }
    throw err;
  })
}


export const submitRegistrationAPI = (formData) => {
  /* your code goes here */
  const urlRegisterUser = usersBackendURL + '/register'
  
  return Axios.post(urlRegisterUser,formData).then(res => {
      return res.data
  }).catch(error=>{
      let err = new Error();
      if(error.response){
        
        err.message = error.response.data.message;
      }
      else{
        err.message = 'Server Error, Please try later';
      }
      throw err
  })
}

export const sellerRegistrationAPI = (formData) => {
  /* your code goes here */
  const urlRegisterSeller = sellerBackendURL + '/register'
  
  return Axios.post(urlRegisterSeller,formData).then(res => {
      // console.log(res);
      
      return res.data
  }).catch(error=>{
      let err = new Error();
      if(error.response){
        err.message = error.response.data.message;
      }
      else{
        // console.log("in api");
        
        err.message = 'Server Error, Please try later';
      }
      throw err
  })
}

export const sellerLoginAPI = (formData) => {
  /* your code goes here */
  const urlLoginSeller = sellerBackendURL + '/login'
  
  return Axios.post(urlLoginSeller,formData).then(res => {
    //  console.log(res.data.data);
      return res.data.data
  }).catch(error=>{
      let err = new Error();
      if(error.response){
        err.message = error.response.data.message;
      }
      else{
        err.message = 'Server Error, Please try later';
      }
      throw err
  })
}

export const addProductsAPI = (formData) => {
  /* your code goes here */
  const urlAddProducts = productsBackendURL + '/addProduct';
  return Axios.post(urlAddProducts,formData).then(res => {
      // console.log(res.data.data.data[0]);
      // console.log(res.data);
      return res.data.data;
  }).catch(error=>{
      let err = new Error();
      if(error.response){
        // console.log(error.response);
        err.message = error.response.data.message;
      }
      else{
        // console.log("in api");
        err.message = 'Server Error, Please try later';
      }
      throw err
  })
}

export const deleteSellerProductAPI=(sellerId,productId)=>{
  var url=sellerBackendURL+"/deleteProduct/"+sellerId+"/"+productId;
  // console.log(url);
  return Axios.get(url)
  .then(res=>
    {
      // console.log("after deletion updated seller object=",res.data.data[0]);
      return res.data.data[0];
    }
    ).catch(err=>
    {
    let e=new Error()
    if(err.response)
    {
      // console.log("error response");
      e.message=err.response.data.message;
    }
    else
    {   
      // console.log("start server error",err);
      e.message="Please start your Express server";
    }
    throw e;
})
}


export const emptyCartAPI=(userId)=>{
  var url=usersBackendURL+'/clearCart/'+userId;
  // console.log(url);
  return Axios.put(url).then(res=>{
      // console.log(res.data.message);
      return res.data.message;
  }).catch(err=>{
    if(err.response.data.message){
        throw err;
    }
    else{
        throw new Error("Please start your Express server")
    }
})
}

export const searchProducts=(search)=>{
  var url=productsBackendURL+"/getProducts/";
  return Axios.get(url+search).then(result=>{
    // console.log("products returned=",result.data);
    return result.data;
  }).catch(err=>{
    let e=new Error();
    if(err.response){
      e.message=err.response.data.message;
    }else{
      e.message="Please start your Express Server!";
    }
    throw e;
  })


}


export const getSellersOfProducts=(productId)=>{
  var url=productsBackendURL+'/getSellerDetails/'+productId;
  // console.log(url);
  return Axios.get(url)
  .then(res=>{
    // console.log(res.data.data[0].sellerDetails);
    return res.data.data[0].sellerDetails;
  }).catch(err=>{
    // console.log(err);
  })
}

export const placeOrder=(userObj)=>{
  var url=usersBackendURL+"/checkout";
  // console.log("in api.js placeOrder");
  return Axios.post(url,userObj).then(res=>{
    //console.log("message recieved on placing order1=",res.data);
    // console.log("message recieved on placing order2=",res.data.userData);
    return res.data.userData;
  }).catch(err=>{
    let e=new Error();
    if(err.response){
      e.message=err.response.data.message;
    }else{
      e.message="Please start your Express Server!";
    }
    throw e;
  })
}


export const sendCartOnLogout=(cart,userId)=>{
  // console.log("in sendCart api.js");
  var url=usersBackendURL+"/updateCart/"+userId;
  return Axios.put(url,cart).then(res=>{
    return res;
  }).catch(err=>{
    let e=new Error();
    if(err.response){
      e.message=err.response.data.message;
    }else{
      e.message="Please start your Express Server!";
    }
    throw e;
  })

}
