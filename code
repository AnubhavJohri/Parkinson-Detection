##SODOC##
import React, { Component } from 'react';
import BookIcon from '@material-ui/icons/Book';
import AddShoppingCartIcon from '@material-ui/icons/AddShoppingCart';
import "../Custom2.css"
import Tabpanel from './Tabpanel'
import Button from 'react-bootstrap/Button'
import ButtonToolbar from 'react-bootstrap/ButtonToolbar'
import { connect } from "react-redux";
import Rating from '@material-ui/lab/Rating';
import Box from '@material-ui/core/Box';
import Axios from 'axios';
import CircularProgress from '@material-ui/core/CircularProgress';
import {Redirect} from 'react-router-dom'


const productsBackendURL = "http://localhost:1111/products";
class SearchBar extends Component{
  constructor(props) {
    super(props);
    this.state = {
        result : [],
        search : this.props.match.params.category, //this.props.location.state.id
        sorryMessage : "",
        loading : false,
        viewDetailsValid : false
    };
}

// fetchSearchResults = () => {
//   handleSearchCategory(this.props.dispatch, this.props.search);
//   this.setState(()=>({
//     result
//   }))
// }

  searchProducts=(search)=>{
  // console.log(this.state.result);
  this.setState(()=>({
    loading : true,
    result : [],
    sorryMessage : ""
  }))
  
  var url=productsBackendURL+"/getProducts/";
  return Axios.get(url+search).then(result=>{
    // console.log("products returned=",result.data.data);
    this.setState(()=>({
      result : result.data.data,
      loading : false,
      sorryMessage : ""
    }))
  }).catch(err=>{
    
    if(err.response.status == 404){
      this.setState(()=>({
        sorryMessage : "Sorry! We do not currently sell " + this.props.match.params.category + ". Please visit us again while we add this. Meanwhile, you can look at our other products.",
        loading : false,
        result : []
      }))
    }else{
      this.setState(()=>({
        loading : false,
        sorryMessage : "Please start your Express Server!",
        result: []
      }))
      
    }
  
  })
}


componentDidMount = () => {
  this.searchProducts(this.state.search);
}

componentDidUpdate = (prevProps) => {
  if (prevProps != this.props) {

    
    this.searchProducts(this.props.match.params.category);
  }
}

// addToCrt = () => {
//   addToCart
// }



displayProducts = () => {
  // return this.state.result.map((element, key) => {
  //     return (
  //         <div className="col-3 offset-1" id={element.productId} key={key}>
  //             <div className="card bg-light cardClass">
  //                 <div classname="card-header card-img-top">
  //                     <img className="imageCardHeader" src={element.productImage} alt={element.productName} />
  //                     <div className="card-body">
  //                         <h3 className="card-title">{element.productName}</h3>
  //                         <p>Best buy: &#x20b9; {element.sellerDetails[0].productPrice}</p>
  //                         <p> <Box component="fieldset" mb={3} borderColor="transparent">

  //                             <Rating value={element.productRatings} readOnly />
  //                         </Box></p>
  //                         <button name="addToCart" className="btn btn-danger btn-block" onClick={() => this.addToCart(element)}>Add To Cart </button>
  //                         <button name="viewDetails" className="btn btn-success btn-block" onClick={() => this.setState({ viewDetailsValid: true, selectedProduct: element })}>View Details</button>
  //                     </div>
  //                 </div>
  //             </div>
  //         </div>
  //     )
  // })

  return this.state.result.map((element, key) => {
          return (
  
            <div className="col-4 " id={element.productId} key={key}>
                <div className="card bg-light cardClass text-center">
                    <div classname="card-header card-img-top ">
                        <img className="imageCardHeader text-center" src={element.productImage} alt={element.productName} />
                        <div className="card-body text-left">
                            <h5 className="card-title" >{element.productName}</h5>
                            <p>{"Best buy:"} <b>&#x20b9; {element.sellerDetails[0].productPrice}</b></p>
                            <p> <Box component="fieldset" mb={0} borderColor="transparent">
                                <Rating value={element.productRatings} readOnly />
                            </Box></p>
                            <p>{"Manufacturer:"}<b>{element.productManufacturer}</b></p>
                          <ButtonToolbar>
                            <Button variant="warning" size = "sm" name="addToCart">Add to <AddShoppingCartIcon /></Button>
                           &nbsp;&nbsp;&nbsp;
                           <Button variant="success" size = "sm" name="viewDetails" onClick={() => this.setState({ viewDetailsValid: true, selectedProduct: element })}>View Details<BookIcon /></Button>
                           </ButtonToolbar>
                        
                            {/* <button name="addToCart" className="btn btn-warning " onClick={() => this.addToCart(element)}>Add To Cart </button>&nbsp;&nbsp; */}

                        </div>
                    </div>
                </div>
            </div>
      )
        
    
})
}


render() {

  if (this.state.viewDetailsValid) {
    // console.log("in products", this.state.selectedProduct);
    //return <Redirect push to={"/ViewProduct/"+this.state.selectedProduct.productName}/>
    return (<Redirect to={{
        pathname: "/ViewProduct/" + this.state.selectedProduct.productName,
        state: { data: this.state.selectedProduct }
    }} />)
}
  return (
    <React.Fragment>
      {/* <NavBar /> */}
        <Tabpanel />
        {this.state.loading ? ( <CircularProgress color = "secondary" size = {100} style={{marginTop : "250px", marginLeft : "650px"}} /> ) : (

        <div className="container"><br /><br />
            <div className="row">
                {/* Invoke the required method Here to render the Products */}
                
                {this.state.result == 0 ? (<h3 className = "text-primary">{this.state.sorryMessage}</h3>) : (
                          this.displayProducts()
                          
                )}
                
               
                
            </div>
        </div>)}
    </React.Fragment>
)
}
}


function mapStateToProps({ authedUser,allProducts }) {
return {
    authedUser,
    allProducts,
}
}
export default connect(mapStateToProps)(SearchBar);
