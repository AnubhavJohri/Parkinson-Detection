import React, { Component } from 'react';
import { MDBCarousel, MDBCarouselInner, MDBCarouselItem, MDBView, MDBContainer } from "mdbreact";
import { handleEmptyCart, handleUpdateCart, handlePlaceOrder, handleSendCartOnLogout } from '../actions/authedUser'
import Tabpanel from './Tabpanel'
import {Link} from 'react-router-dom'
import { connect } from "react-redux";
import { log } from 'util';

class Home extends Component {
  constructor(props) {
    super(props);
    this.state = {
        data: this.props.authedUser.data[0],
        counter: "",
        msg: ""
    }
}

  // componentDidMount(){
  //   var stringUserObject=JSON.stringify(this.props.authedUser);
  //   sessionStorage.setItem("uEmail",stringUserObject);
  //   var userObject=sessionStorage.getItem("uEmail");
  //   console.log("User object in didmount of home",this.props.authedUser);
  //   console.log("User object in didmount of home",userObject);
  // }
  componentWillUnmount(){
    // console.log("dashboard did unmount");
    if(this.state!=null && this.state.data!==undefined  &&Object.keys(this.state.data).length!=0 ){
      //console.log("dashboard did unmount",this.state.data.cart);
        var hpAuthedUser=this.state.data
        var stringUserObject=JSON.stringify(hpAuthedUser);
        sessionStorage.setItem("uEmail",stringUserObject);
        handleSendCartOnLogout(this.props.dispatch,this.state.data.cart,this.state.data.userId)
        //handleSendCartOnLogout(this.props.dispatch,this.state.data.cart,this.state.data.userId)
    }
}
  render() {
    return (
      <React.Fragment><Tabpanel />
      <MDBContainer>
      <MDBCarousel
        activeItem={1}
        length={4}
        showControls={true}
        showIndicators={true}
        className="z-depth-1"
        slide
      >
        <MDBCarouselInner>
          <MDBCarouselItem itemId="1">
            <MDBView>
              <Link to = '/products/getProducts/Clothing'>
              <img
                className="d-block w-100"
                src={require("../assets/img/clothing.png")}
                alt="Clothing Pic"
              /></Link>
            </MDBView>
          </MDBCarouselItem>
          <MDBCarouselItem itemId="2">
            <MDBView>
            <Link to = '/products/getProducts/Furniture'>
              <img
                className="d-block w-100"
                src={require("../assets/img/furnitures.png")}
                alt="Furniture Pic"
              /></Link>
            </MDBView>
          </MDBCarouselItem>
          <MDBCarouselItem itemId="3">
            <MDBView>
            <Link to = '/products/getProducts/Shoes'>
              <img
                className="d-block w-100"
                src={require("../assets/img/shoes.png")}
                alt="Shoes Pic"
              /></Link>
            </MDBView>
          </MDBCarouselItem>
          <MDBCarouselItem itemId="4">
            <MDBView>
            <Link to = '/products/getProducts/Electronics'>
              <img
                className="d-block w-100"
                src={require("../assets/img/Electronics.jpg")}
                alt="Mobile Pic"
              /></Link>
            </MDBView>
          </MDBCarouselItem>
        </MDBCarouselInner>
      </MDBCarousel>
    </MDBContainer>
    </React.Fragment>
    );
  }
}
