filepath:Project\hoopla_products-master\src\utilities\Validator.js##EOFP##
filename:Validator.js
##SODOC##
const validator={}
validator.checkproductCategory=function(productCategory){
    if(productCategory.match(/^[A-Za-z]+$/)==null){
        let err=new Error("Product category name should only contain alphabets");
        err.message="Product category name should only contain alphabets";
        throw err;
    }
}

validator.checkproductId=function(productId){
    if(productId.match(/^[P]{1}[0-9]{3}$/)==null){
        let err=new Error("Product Id should be of format P1001, P222");
        err.message="Product Id should be of format P1001, P222";
        throw err;
    }
}

validator.checkManufacturer=function(productManufacturer){
    if(productManufacturer.match(/^[A-Za-z0-9]{1}[" "A-Za-z0-9]*[A-Za-z0-9]{1}$/)==null){
        let err=new Error("Product manufacturer can only have alphabets");
        err.message="Product manufacturer can only have alphabets";
        throw err;
    }
}

validator.checkProductName=function(productName){
    if(productName.match(/^[A-Za-z0-9]{1}[" "A-Za-z0-9]*[A-Za-z0-9]{1}$/)==null){
        let err=new Error("Product name can only have alphabets and digits and spaces");
        err.message="Product name can only have alphabets and digits";
        throw err;
    }
}


module.exports=validator;
