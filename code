##SODOC##
const validator = require('../src/utilities/Validator');
const ErrorLogger = require('../src/utilities/errorLogger');
const req = require('request')

describe('Testing Validator', () => {
    it('validateSellerId: correct Input', () => {
        expect(validator.checkSId("S1001")).toEqual(undefined);
    })
    it('validateSellerId: correct Input', () => {
        expect(validator.checkSId("S1021")).toEqual(undefined);
    })
    it('validateSellerId: correct Input', () => {
        expect(validator.checkSId("S1041")).toEqual(undefined);
    })
    it('validateSellerId: correct Input', () => {
        expect(validator.checkSId("S1401")).toEqual(undefined);
    })
    it('validateAccountNumber: correct Input', () => {
        expect(validator.checkAccNo("100211211218")).toEqual(undefined);
    })
    it('validateAccountNumber: correct Input', () => {
        expect(validator.checkAccNo("100211211215")).toEqual(undefined);
    })
    it('validateAccountNumber: correct Input', () => {
        expect(validator.checkAccNo("100211210218")).toEqual(undefined);
    })
    it('validateAccountNumber: correct Input', () => {
        expect(validator.checkAccNo("100211101218")).toEqual(undefined);
    })
    it('validateTAN: correct Input', () => {
        expect(validator.checkTAN("AAAA12305A")).toEqual(undefined);
    })
    it('validateTAN: correct Input', () => {
        expect(validator.checkTAN("AAAA22345A")).toEqual(undefined);
    })
    it('validateTAN: correct Input', () => {
        expect(validator.checkTAN("AAAA12545A")).toEqual(undefined);
    })
    it('validateTAN: correct Input', () => {
        expect(validator.checkTAN("AAAA19345A")).toEqual(undefined);
    })
    it('validateGST: correct Input', () => {
        expect(validator.checkGST("12ABCDE1234A1Z1")).toEqual(undefined);
    })
    it('validateGST: correct Input', () => {
        expect(validator.checkGST("12ABCDE1234A1N1")).toEqual(undefined);
    })
    it('validateGST: correct Input', () => {
        expect(validator.checkGST("12ABCDE1204A1Z1")).toEqual(undefined);
    })
    it('validateGST: correct Input', () => {
        expect(validator.checkGST("12ABCDE1294A1L1")).toEqual(undefined);
    })

    it('validateSellerid: incorrect Input', () => {
        expect(function () { validator.checkSId("1001") }).toThrow(new Error("Seller Id not in format (S1001)"));
    })
    it('validateTAN: incorrect Input', () => {
        expect(function () { validator.checkTAN("AAA1345A") }).toThrow(new Error("The TAN should be of format: AAAA12345A"));
    })
    it('validateGST: incorrect Input', () => {
        expect(function () { validator.checkGST("12ABCDE1234A1") }).toThrow(new Error("The GST should be of format:12ABCDE1234A1Z1"));
    })
    it('validateProductId: incorrect Input', () => {
        expect(function () { validator.checkAccNo("25890101") }).toThrow(new Error("Account Number must be 9-18 characters"));
    })
   
})

describe('Testing ErrorLogger', () => {
    let obj = { "errorLogger": ErrorLogger }
    let next = () => { }
    beforeEach(() => {
        res = {
            status: undefined,
            json: undefined,
            status: function (status) {
                this.status = status
            },
            json: function (val) {
                this.json = val
            }
        }
    })

    it('Response status set to 500 by default for Error', () => {
        let err = new Error("ErrorLogger test");
        try {
            obj.errorLogger(err, req, res, next)
        } catch (e) { }
        finally {
            expect(res.status).toEqual(500);
        }
    })


    it('Response status is set using Error status', () => {
        let err = new Error("ErrorLogger test");
        err.status = 404
        try {
            obj.errorLogger(err, req, res, next)
        } catch (e) { }
        finally {
            expect(res.status).toEqual(404);
        }
    })

    it('Response Should Contain Error Message', () => {
        let err = new Error("ErrorLogger test");
        try {
            obj.errorLogger(err, req, res, next)
        } catch (e) { }
        finally {
            expect(res.json.message).toEqual(err.message);
        }
    })
})
