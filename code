##SODOC##
const{ Schema } = require( 'mongoose' );
const mongoose = require( 'mongoose' );
mongoose.Promise = global.Promise;
mongoose.set( 'useCreateIndex', true );
const url = "mongodb://localhost:27017/sellerDB"


const sellerSchema = Schema( {
    sellerId: { type: String, required: [true, 'sellerId is required'], unique: true },
    sellerName: { type: String, required: [true, 'sellerName is required'] },
    sellerEmail: { type: String, required: [true, 'sellerEmail is required'], unique: true },
    sellerPhone: { type: Number, required: [true, 'sellerPhone is required'] },
    sellerAccNo: { type: Number, required: [true, 'sellerAccNo is required'], unique:true },
    sellerPass: { type: String, required: [true, 'sellerPass is required'] },
    sellerTAN: { type: String, required: [true, 'sellerTan is required'], unique: true },
    sellerGST: { type: String, required: [true, 'sellerGST is required'], unique: true },
    sellerProducts: [{
        productId: { type: String, unique: true },
        productQuantity: { type: Number },
        price:{type:Number},
        discount:{type:Number},
        productName:{type:String}
    }]

}, { collection: "Seller", timestamps: true } )

let connection = {}

//Returns model object of "Sellers" collection
connection.getCollection = () => {
    //Establish connection and return model as promise
    return mongoose.connect( url, { useNewUrlParser: true } ).then( database => {
        
        return database.model( 'Seller', sellerSchema )
    } ).catch( error => {
        
        let err = new Error( "Could not connect to the database" );
        err.status = 500;
        throw err;
    } );
}

module.exports = connection;
