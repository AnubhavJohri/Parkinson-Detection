##SODOC##
describe("User Register Component", () => {
    it("Should register a user on entering correct values", () => {
            cy.visit("http://localhost:3000/register")
            cy.get('form[name="submit"]').then(()=>{
            cy.get('input[name="name"]').type("Anubhav")
            cy.get('input[id="mNumber"]').type("9958056076")
            cy.get('input[name="address"]').type('ghaziabad c-block')
            cy.get('input[name="email"]').type('anubhavj22@gmail.com')
            cy.get('input[name="password"]').type('John@123')
            cy.get('input[name="dob"]').type('2016-01-01')
            cy.get('form[name="submit"]').submit()
            })
    })
    it("Should not register a user on entering and email Id that already exists", () => {
        cy.visit("http://localhost:3000/register")
        cy.get('form[name="submit"]')
        .then(()=>{
            cy.get('input[name="name"]').type("Anubhav")
            cy.get('input[id="mNumber"]').type("9958056076")
            cy.get('input[name="address"]').type('ghaziabad c-block')
            cy.get('input[name="email"]').type('anubhavj22@gmail.com')
            cy.get('input[name="password"]').type('John@123')
            cy.get('input[name="dob"]').type('2016-01-01')
            cy.get('form[name="submit"]').submit()
            cy.wait(5000)
            cy.get('p[class="text-danger"]').then((errorMessage) => {
                var c=errorMessage[0].innerHTML;
                var st="Sorry this email id is already registered.";
                console.log("===",errorMessage[0]);
                expect(c).to.eq(st)
            })
        })


})
it("Should show error on adding wrong emailId", () => {
    cy.visit("http://localhost:3000/register")
    cy.get('form').then(()=>{
    cy.get('input[name="email"]').type('anubhavj22')
    cy.get('span[name="emailError"]').then((email) => {
        var c=email[0].innerHTML;
        var st="​Email Id must be of format John@example.com";
        //console.log(c===st.substring(1));
        expect(email[0].innerHTML).to.eq(st.substring(1))
    })
})
})
})
