        "discount":10
    },{
        "productId": "P1022",
        "productQuantity": 100,
        "productName": "Rajpura Dining Set",
        "price":14778,
        "discount":10
    },{
        "productId": "P1023",
        "productQuantity": 100,
        "productName": "Solid Denim Jacket",
        "price":8499,
        "discount":54
    }
]
}]

let seller = {}
seller.setupDB = () => {
    //console.log("dbSetup called.")
    return collection.getCollection().then(sellerColl => {
      //  console.log("seller collection accessed")
        return sellerColl.deleteMany().then(data => {
          //  console.log("Now data is: ",data);
            return sellerColl.insertMany(sellerData).then(result => {
                if (result && result.length > 0) return result.length
                else return null
            });
        });
    });
}

module.exports = seller
