filename:App.js
##SODOC##
import React, { Component, Fragment } from 'react';
import { BrowserRouter as Router, Route, Switch ,Redirect} from 'react-router-dom';
import './App.css';
import Navbar from "./NavBar";
import Home from "./Home";
import Login from "./Login";
import Register from './Register'
import Cart from './Cart'
import Orders from './Orders' 
import PastOrder from './PastOrder'
import Products from './Products'
import SellerRegistration from './SellerRegistration'
import Search from './Search';
import SellerProducts from './SellerProducts';
import SellerLogin from './SellerLogin';
import AddProducts from './AddProducts';
import ViewProduct from './ViewProduct';


class App extends Component {

  render() {
    return (
      <div>
        <Router>
          <Fragment>
            <Navbar />
            
            <Switch>
              <Route exact path='/dashboard' component={Home} />
              <Route exact path='/login' component={Login} />
              <Route exact path='/sellerRegistration' component={SellerRegistration} />
              <Route exact path='/register' component={Register} />
              <Route exact path='/cart' component={Cart} />
              <Route exact path='/orders' component={Orders} />
              <Route exact path='/search/:category' component={Search} />
              <Route exact path='/PastOrders' component={PastOrder} />
              <Route exact path='/SellerProducts' component={SellerProducts} />
              <Route path='/products/getProducts' component={Products} />
              <Route exact path='/SellerLogin' component={SellerLogin} />
              <Route exact path='/AddProducts' component={AddProducts} />
              <Route exact path='/ViewProduct/:productName' component={ViewProduct} />
              <Route exact path='/**' render={()=><Redirect to="/dashboard"/>} />

            </Switch>
          </Fragment>
        </Router>
      </div>
    );
  }
}

export default App
##EODOC##



