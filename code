filename:Products.js
##SODOC##
import React, { Component } from "react";
import "./App.css";
import Axios from "axios";
import Button from 'react-bootstrap/Button'
import ButtonToolbar from 'react-bootstrap/ButtonToolbar'
import CircularProgress from '@material-ui/core/CircularProgress';

import TabPanel from './Tabpanel'
import { BrowserRouter as Router, Route, Switch, Redirect } from 'react-router-dom';
import AddShoppingCartIcon from '@material-ui/icons/AddShoppingCart';
import '../Custom2.css'
import Rating from '@material-ui/lab/Rating';
import Box from '@material-ui/core/Box';
import { connect } from "react-redux";
import { handleEmptyCart, handleUpdateCart, handleGetSeller, setHPAuthedUser, handleSetHPAuthedUser, handleSetHPAuthedUserCrate, handleSetErrors } from '../actions/authedUser'
import '../Custom2.css'
import withStyles from '@material-ui/core/styles/withStyles';
import PropTypes from 'prop-types';
import { yellow } from "@material-ui/core/colors";
import BookIcon from '@material-ui/icons/Book';


const url = "http://localhost:1111";

const styles = theme => ({
    button: {
        margin: theme.spacing(1),
        

    },
    input: {
        display: 'none',
    },
});



/* DO NOT ADD REMOVE OR ALTER ANY STATE OF THE COMPONENT */
class Products extends Component {
    constructor(props) {
        super(props);
        this.state = {
            products: [],
            errorMessage: "",
            selectedProduct: null,
            viewDetailsValid: false,
            data: {},
            Sellers: [],
            selectedSellerName: "",
            loading : false,
            cart: {
                "productId": "",
                "productName": "",
                "quantity": 0,
                "price": 0,
                "product_img_url": "",
                "sellerName": "",
                "discount": 0,
            },
            formValid: false,
            flag: 0,
            counter: 0,
            notLoggedATCPressed : false
            
        };
    }

    fectProductDetails = () => {
        this.setState(() => ({
            products: [],
            errorMessage: "",
            loading : true
        }))
        // console.log(url + this.props.location.pathname);
        
        return Axios.get(url + this.props.location.pathname).then(success => {
            // console.log("data=============", success.data.data[0])
            this.setState(() => ({ products: success.data.data, loading : false }))
            return success.data.data;
        }).catch(err => {
            if (err.response) {
                //console.log(err.response);
                this.setState(() => ({
                    errorMessage: err.response.data.message,
                    loading : false
                }))
            }
            else {
                this.setState(() => ({
                    errorMessage: "Server Error",
                    loading : false
                }))
            }
        })

    }

    getAllSellers = (productId) => {
        //console.log("productId of selected product",productId);
        handleGetSeller(this.props.dispatch, productId).then(arr => {
            this.setState({ Sellers: arr });
        })
    }

    componentDidMount = () => {
        //console.log("did mount");
        this.fectProductDetails().then(res => {
            //console.log("response ===",res);
            if (res && res.length !== 0 ) {
                //console.log(123456);
                
                handleSetErrors(this.props.dispatch, "", "")
                if(res[0].sellerDetails.length>0 && res[0].sellerDetails[0].s_name){this.setState({ selectedSellerName: res[0].sellerDetails[0].s_name })}
                this.getAllSellers(res[0].productId);
            } else {
                //console.log("Service Unavailable");
                handleSetErrors(this.props.dispatch, "", "Service Unavailable.Start your server")
            }
        })
    }

    componentDidUpdate = (prevProps) => {
        if (prevProps != this.props) {
            this.fectProductDetails();
        }
    }

    displayErrors = () => {
        return <h1 className="text-danger">{this.props.Messages.errorMessage}</h1>
    }

    displayProducts = () => {
        const { classes } = this.props;


        return this.state.products.map((element, key) => {
            //console.log("ele=",element.sellerDetails.length);
            if (element.sellerDetails && element.sellerDetails.length != 0) {
                                return (
                    <div className="col-4 " id={element.productId} key={key}>
                        <div className="card bg-light cardClass text-center">
                            <div classname="card-header card-img-top ">
                                <img className="imageCardHeader text-center" src={element.productImage} alt={element.productName} />
                                <div className="card-body text-left">
                                    <h5 className="card-title" >{element.productName}</h5>
                                    <p>{"Best buy:"} <b>&#x20b9; {element.sellerDetails[0].productPrice}</b></p>
                                    <p> <Box component="fieldset" mb={0} borderColor="transparent">
                                        <Rating value={element.productRatings} readOnly />
                                    </Box></p>
                                    <p>{"Manufacturer:"}<b>{element.productManufacturer}</b></p>
                                  <ButtonToolbar>
                                    <Button variant="warning" size = "sm" onClick={() => this.addToCart(element)} name="addToCart">Add to <AddShoppingCartIcon /></Button>
                                   &nbsp;&nbsp;&nbsp;
                                   <Button variant="success" size = "sm" name="viewDetails" onClick={() => this.setState({ viewDetailsValid: true, selectedProduct: element })}>View Details<BookIcon /></Button>
                                   </ButtonToolbar>
                                
                                    {/* <button name="addToCart" className="btn btn-warning " onClick={() => this.addToCart(element)}>Add To Cart </button>&nbsp;&nbsp; */}

                                </div>
                            </div>
                        </div>
                    </div>
                )
            }
        })
    }

    addToCart = (selectedProduct) => {
        // this.setState(() => ({
        //             selectedProduct: selectedProduct
        //         }))
        if((this.props.authedUser.data.length > 0)&&(this.props.authedUser.data[0])&&(this.props.authedUser.data[0].uCredentials!=undefined) ){
        var flag = 0;
        this.makeCartObject(selectedProduct);
        var authedUser = this.props.authedUser.data[0];
        //console.log("before",this.state.cart,authedUser);
        for (let i in authedUser.cart) {
            //console.log("newcarts=", authedUser.cart[i].productName, this.state.cart.productName);
            if (authedUser.cart[i].productName == this.state.cart.productName) {
                authedUser.cart[i].quantity += 1;
                flag = 1;
                break;
            }
        }
        if (flag == 0) {
            authedUser.cart.push(this.state.cart)
        }
        handleSetHPAuthedUserCrate(this.props.dispatch, authedUser);
    }
    else{
        this.setState(()=>({
            notLoggedATCPressed : true
        }))
    }
}

    makeCartObject = (selectedProduct) => {
        const { productImage, productName, productDescription, productManufacturer, productSpecs, productRatings, productId } = selectedProduct;
        var cart = this.state.cart;
        cart["productId"] = productId;
        cart["productName"] = productName;
        cart["product_img_url"] = productImage;
        var flag = 0;
        //console.log("sellername=", this.state.products[0].sellerDetails[0].s_name);
        for (let i in this.state.Sellers) {
            //console.log("sellers=", this.state.Sellers[i].s_name, this.state.products[0].sellerDetails[0].s_name);
            if (this.state.Sellers[i].s_name === this.state.products[0].sellerDetails[0].s_name) {
                //console.log("seller details=", this.state.Sellers[i].s_name, this.state.Sellers[i].productPrice, this.state.Sellers[i].productDiscount, this.state.Sellers[i].productQuantity);
                cart["sellerName"] = this.state.Sellers[i].s_name;
                cart["quantity"] = 1;
                cart["price"] = this.state.Sellers[i].productPrice;
                cart["discount"] = this.state.Sellers[i].productDiscount;
                this.setState({ cart: cart, flag: 1 })
            }
        }
    }

    render() {
        const { classes } = this.props;
        if (this.state.viewDetailsValid) {
            //console.log("in products", this.state.selectedProduct);
            //return <Redirect push to={"/ViewProduct/"+this.state.selectedProduct.productName}/>
            return (<Redirect to={{
                pathname: "/ViewProduct/" + this.state.selectedProduct.productName,
                state: { data: this.state.selectedProduct }
            }} />)
        }
        else if(this.state.notLoggedATCPressed){
            return <Redirect to = '/login' />
        }

        return (
            <React.Fragment>
                <TabPanel />
        {this.state.loading ? ( <CircularProgress color = "secondary" size = {100} style={{marginTop : "250px", marginLeft : "650px"}} /> ) : (

        
                <div className="container"><br /><br />
                    <div className="row">
                        {/* Invoke the required method Here to render the Products */}
                        {(this.props.Messages && this.props.Messages.errorMessage && this.props.Messages.errorMessage.length != 0) ? this.displayErrors() : this.displayProducts()}
                        {/* {this.displayProducts()} */}
                    </div>
                </div>)}
            </React.Fragment>
        )
    }
}

Products.propTypes = {
    classes: PropTypes.object.isRequired,
};

function mapStateToProps({ authedUser, authedSeller, Messages }) {
    return {
        authedUser,
        authedSeller,
        Messages
    }
}
export default connect(mapStateToProps)(withStyles(styles)(Products));
