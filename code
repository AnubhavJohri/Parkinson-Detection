##SODOC##
const validator = require('../src/utilities/Validator');
const ErrorLogger = require('../src/utilities/errorLogger');
const reqlogger=require('../src/utilities/errorLogger');
const req = require('request')

describe('Testing Validator', () => {
    it('validateProductCategory: correct Input', () => {
        expect(validator.checkproductCategory("shoes")).toEqual(undefined);
    })
    it('validateProductCategory: correct Input', () => {
        expect(validator.checkproductCategory("shoes")).toEqual(undefined);
    })
    it('validateProductCategory: correct Input', () => {
        expect(validator.checkproductCategory("electronics")).toEqual(undefined);
    })
    it('validateProductCategory: correct Input', () => {
        expect(validator.checkproductCategory("furniture")).toEqual(undefined);
    })
    it('validateProductCategory: correct Input', () => {
        expect(validator.checkproductCategory("clothing")).toEqual(undefined);
    })

    it('validateProductCategory: incorrect Input', () => {
        expect(function () { validator.checkproductCategory("101") }).toThrow(new Error("Product category name should only contain alphabets"));
    })
    it('validateProductCategory: incorrect Input', () => {
        expect(function () { validator.checkproductCategory("P101") }).toThrow(new Error("Product category name should only contain alphabets"));
    })
    it('validateProductId: correct Input', () => {
        expect(validator.checkproductId("P101")).toEqual(undefined);
    })
    it('validateProductId: correct Input', () => {
        expect(validator.checkproductId("P121")).toEqual(undefined);
    })
    it('validateProductId: correct Input', () => {
        expect(validator.checkproductId("P142")).toEqual(undefined);
    })
    it('validateProductId: correct Input', () => {
        expect(validator.checkproductId("P111")).toEqual(undefined);
    })
    it('validateProductId: incorrect Input', () => {
        expect(function () { validator.checkproductId("P1010") }).toThrow(new Error("Product Id should be of format P1001, P222"));
    })
    it('validateProductId: incorrect Input', () => {
        expect(function () { validator.checkproductId("O101") }).toThrow(new Error("Product Id should be of format P1001, P222"));
    })
    it('validateProductManufacturer: correct Input', () => {
        expect(validator.checkManufacturer("levis")).toEqual(undefined);
    })
    it('validateProductManufacturer: correct Input', () => {
        expect(validator.checkManufacturer("levis 69")).toEqual(undefined);
    })
    it('validateProductManufacturer: correct Input', () => {
        expect(validator.checkManufacturer("levis to cool")).toEqual(undefined);
    })
    it('validateProductManufacturer: correct Input', () => {
        expect(validator.checkManufacturer("samsung")).toEqual(undefined);
    })
    it('validateProductManufacturer: incorrect Input', () => {
        expect(function () { validator.checkManufacturer("P1010@") }).toThrow(new Error("Product manufacturer can only have alphabets"));
    })
    it('validateProductManufacturer: incorrect Input', () => {
        expect(function () { validator.checkManufacturer("P1010 ") }).toThrow(new Error("Product manufacturer can only have alphabets"));
    })
    it('validateProductManufacturer: incorrect Input', () => {
        expect(function () { validator.checkManufacturer("levis69 ") }).toThrow(new Error("Product manufacturer can only have alphabets"));
    })

    it('validateProductName: correct Input', () => {
        expect(validator.checkProductName("samsung")).toEqual(undefined);
    })
    it('validateProductName: correct Input', () => {
        expect(validator.checkProductName("levis")).toEqual(undefined);
    })
    it('validateProductName: correct Input', () => {
        expect(validator.checkProductName("levis69")).toEqual(undefined);
    })
    it('validateProductName: correct Input', () => {
        expect(validator.checkProductName("levis 69")).toEqual(undefined);
    })
    it('validateProductName: correct Input', () => {
        expect(validator.checkProductName("levis cool")).toEqual(undefined);
    })
    it('validateProductName: correct Input', () => {
        expect(validator.checkProductName("levis cool 69")).toEqual(undefined);
    })
    it('validateProductName: incorrect Input', () => {
        expect(function () { validator.checkProductName("P1010 ") }).toThrow(new Error("Product name can only have alphabets and digits"));
    })
    it('validateProductName: incorrect Input', () => {
        expect(function () { validator.checkProductName("levis69@") }).toThrow(new Error("Product name can only have alphabets and digits"));
    })
    it('validateProductName: incorrect Input', () => {
        expect(function () { validator.checkProductName("levis69@") }).toThrow(new Error("Product name can only have alphabets and digits"));
    })
})

describe('Testing ErrorLogger', () => {
    let obj = { "errorLogger": ErrorLogger }
    let next = () => { }
    beforeEach(() => {
        res = {
            status: undefined,
            json: undefined,
            status: function (status) {
                this.status = status
            },
            json: function (val) {
                this.json = val
            }
        }
    })

    it('Response status set to 500 by default for Error', () => {
        let err = new Error("ErrorLogger test");
        try {
            obj.errorLogger(err, req, res, next)
        } catch (e) { }
        finally {
            expect(res.status).toEqual(500);
        }
    })


    it('Response status is set using Error status', () => {
        let err = new Error("ErrorLogger test");
        err.status = 404
        try {
            obj.errorLogger(err, req, res, next)
        } catch (e) { }
        finally {
            expect(res.status).toEqual(404);
        }
    })

    it('Response Should Contain Error Message', () => {
        let err = new Error("ErrorLogger test");
        try {
            obj.errorLogger(err, req, res, next)
        } catch (e) { }
        finally {
            expect(res.json.message).toEqual(err.message);
        }
    })
})
