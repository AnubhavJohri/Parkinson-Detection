filepath:Project\hoopla_uinew1-master\src\components\Register.js##EOFP##
filename:Register.js
##SODOC##
import React, { Component, Fragment } from 'react';
import Avatar from '@material-ui/core/Avatar';
import Button from '@material-ui/core/Button';
import CssBaseline from '@material-ui/core/CssBaseline';
import TextField from '@material-ui/core/TextField';
import FormControlLabel from '@material-ui/core/FormControlLabel';
import Checkbox from '@material-ui/core/Checkbox';
// import Link from '@material-ui/core/Link';
import Grid from '@material-ui/core/Grid';
import Paper from '@material-ui/core/Paper';

import Box from '@material-ui/core/Box';
import LockOutlinedIcon from '@material-ui/icons/LockOutlined';
import Typography from '@material-ui/core/Typography';
import { makeStyles } from '@material-ui/core/styles';
import Container from '@material-ui/core/Container';
import { handleSubmitRegistration, setMessageAction } from '../actions/authedUser';
import { connect } from 'react-redux'
import PropTypes from 'prop-types';
import withStyles from '@material-ui/core/styles/withStyles';
import Login from './Login';
import { Link } from 'react-router-dom'





const styles = theme => ({
    '@global': {
        body: {
            backgroundColor: theme.palette.common.white,
        },
    },
    paper: {
        marginTop: theme.spacing(8),
        display: 'flex',
        flexDirection: 'column',
        alignItems: 'center',
    },
    avatar: {
        margin: theme.spacing(1),
        backgroundColor: theme.palette.secondary.main,
    },
    form: {
        width: '100%', // Fix IE 11 issue.
        marginTop: theme.spacing(3),
    },
    submit: {
        margin: theme.spacing(3, 0, 2),
    },
});


class Register extends Component {
    constructor(props) {
        super(props);
        this.state = {
            form: {
                name: "",

                mNumber: "",
                dob: "",
                address: "",
                email: "",
                password: "",

            },
            formErrorMessage: {
                name: "",

                mNumber: "",
                dob: "",
                address: "",
                email: "",
                password: "",
            },
            formValid: {
                name: false,

                mNumber: false,
                dob: false,
                address: false,
                email: false,
                password: false,
                buttonActive: false,
            },
        }
    }

    Copyright = () => {
        return (
            <Typography variant="body2" color="textSecondary" align="center">
                {'Copyright © '}
                <Link color="inherit" href="/dashboard">
                    Hoopla
      </Link>{' '}
                {new Date().getFullYear()}
                {'.'}
            </Typography>
        );
    }


    handleChange = (event) => {
        const value = event.target.value;
        const name = event.target.name;
        this.setState((prevState) => ({
            form: {
                ...prevState.form,
                [name]: value
            }
        }))
        this.validateField(name, value)
    }

    //formValidations
    validateField = (fieldName, value) => {
        const newFormErrorMsg = this.state.formErrorMessage;
        const newFormValid = this.state.formValid;

        switch (fieldName) {
            case 'name':
                if (value == "") {
                    newFormErrorMsg.name = "Field required"
                    newFormValid.name = false
                }

                else if (!value.match(/^[a-zA-Z]+([ ]*[a-zA-Z]+)*$/)) {
                    newFormErrorMsg.name = "Name can contain only alphabets and spaces"
                    newFormValid.name = false
                }
                else {
                    newFormErrorMsg.name = ""
                    newFormValid.name = true
                }
                break;

            case 'email':
                if (value == "") {
                    newFormErrorMsg.email = "Field required"
                    newFormValid.email = false
                }

                else if (!value.match(/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/)) {
                    newFormErrorMsg.email = "Email Id must be of format John@example.com"
                    newFormValid.email = false
                }
                else {
                    newFormErrorMsg.email = ""
                    newFormValid.email = true
                }
                break;

            case 'address':
                if (value == "") {
                    newFormErrorMsg.address = "Field required"
                    newFormValid.address = false
                }

                else if (value.length < 10) {
                    newFormErrorMsg.address = "Address must be at least 10 characters"
                    newFormValid.address = false
                }
                else {
                    newFormErrorMsg.address = ""
                    newFormValid.address = true
                }
                break;

            case 'mNumber':
                if (value == "") {
                    newFormErrorMsg.mNumber = "Field required"
                    newFormValid.mNumber = false
                }

                else if (value.length != 10) {
                    newFormErrorMsg.mNumber = "Mobile Number should be of 10 digits"
                    newFormValid.mNumber = false
                }
                else {
                    newFormErrorMsg.mNumber = ""
                    newFormValid.mNumber = true
                }
                break;

            case 'dob':
                const newValue = new Date(value);
                const today = new Date();
                if (value == "") {
                    newFormErrorMsg.dob = "Field required"
                    newFormValid.dob = false
                }

                else if (newValue > today) {
                    newFormErrorMsg.dob = "D.O.B cannot be greater than today"
                    newFormValid.dob = false
                }
                else {
                    newFormErrorMsg.dob = ""
                    newFormValid.dob = true
                }
                break;

            case 'password':
                if (value == "") {
                    newFormErrorMsg.password = "Field required"
                    newFormValid.password = false
                }

                else if (!value.match(/^(?=.*[a-z])(?=.*[A-Z])(?=.*\d)(?=.*[@$!%*?&])[A-Za-z\d@$!%*?&]{7,20}$/)) {
                    newFormErrorMsg.password = "Weak Password"
                    newFormValid.password = false
                }
                else {
                    newFormErrorMsg.password = ""
                    newFormValid.password = true
                }
                break;

            default:
                break;

        }
        this.setState(() => ({
            formErrorMessage: newFormErrorMsg,
            formValid: newFormValid,
        }));

        if (newFormValid.name && newFormValid.mNumber && newFormValid.dob && newFormValid.address && newFormValid.password && newFormValid.email) {
            this.setState((prevState) => ({
                formValid: {
                    ...prevState.formValid,
                    buttonActive: true
                }
            }));
        }
        else {

            this.setState((prevState) => ({
                formValid: {

                    ...prevState.formValid,
                    buttonActive: false
                }
            }));
        }

    }

    submitRegistraion = () => {
        const { email, password, name, dob, mNumber, address } = this.state.form
        const userRegData = { uEmail: email, uPass: password, uName: name, uDOB: dob, uPhone: mNumber, uAddress: address }
        handleSubmitRegistration(this.props.dispatch, userRegData)
    }

    handleSubmit = event => {
        event.preventDefault();
        this.submitRegistraion();
    }

    componentWillUnmount = () => {
        this.props.dispatch(setMessageAction("", ""))
    }


    render() {
        const { classes } = this.props;
        const { form, formValid, formErrorMessage } = this.state;

        return (
            <Fragment>
                {(this.props.successMessage != "") ? <Login /> :
                    (<Container component="main" maxWidth="xs">
                        <CssBaseline />
                        <div className={classes.paper}>

                            <Avatar className={classes.avatar}>
                                <LockOutlinedIcon />
                            </Avatar>
                            <Typography component="h1" variant="h5">
                                Customer Registration!
        </Typography></div>

                        <div>
                            <form className={classes.form} name="submit" onSubmit={this.handleSubmit}>

                                <Grid container spacing={2}>
                                    <Grid item xs={12}>
                                        <TextField
                                            autoComplete="fname"
                                            onChange={this.handleChange}
                                            name="name"
                                            variant="outlined"
                                            required
                                            fullWidth
                                            id="name"
                                            label="Name"
                                            value={form.name}
                                            autoFocus
                                        />
                                        <span className="text-danger">{formErrorMessage.name}</span>
                                    </Grid>
                                    <Grid item xs={12}>
                                        <TextField
                                            variant="outlined"
                                            required
                                            fullWidth
                                            id="mNumber"
                                            label="Mobile Number"
                                            type="number"
                                            name="mNumber"
                                            onChange={this.handleChange}
                                            value={form.mNumber}
                                            autoComplete="nNumberd"
                                        />
                                        <span className="text-danger">{formErrorMessage.mNumber}</span>

                                    </Grid>
                                    <Grid item xs={12}>
                                        <TextField
                                            variant="outlined"
                                            required
                                            fullWidth
                                            id="dob"

                                            label=""
                                            type="date"
                                            name="dob"
                                            onChange={this.handleChange}
                                            value={form.dob}
                                            autoComplete="dob"
                                        />
                                        <span className="text-danger">{formErrorMessage.dob}</span>

                                    </Grid>
                                    <Grid item xs={12}>
                                        <TextField
                                            variant="outlined"
                                            required
                                            fullWidth
                                            id="address"
                                            label="Address"
                                            type="text"
                                            name="address"
                                            onChange={this.handleChange}
                                            value={form.address}
                                            autoComplete="address"
                                        />
                                        <span className="text-danger">{formErrorMessage.address}</span>

                                    </Grid>
                                    <Grid item xs={12}>
                                        <TextField
                                            variant="outlined"
                                            required
                                            fullWidth
                                            id="email"
                                            label="Email Address"
                                            name="email"
                                            type="email"
                                            onChange={this.handleChange}
                                            value={form.email}
                                            autoComplete="email"
                                        />
                                        <span name='emailError' className="text-danger">{formErrorMessage.email}</span>

                                    </Grid>
                                    <Grid item xs={12}>
                                        <TextField
                                            variant="outlined"
                                            required
                                            fullWidth
                                            name="password"
                                            label="Password"
                                            type="password"
                                            id="password"
                                            onChange={this.handleChange}
                                            value={form.password}
                                            autoComplete="current-password"

                                        />
                                        <span className="text-danger">{formErrorMessage.password}</span>

                                    </Grid>
                                    {/* <Grid item xs={12}>
                                    <FormControlLabel
                                        control={<Checkbox value="allowExtraEmails" color="primary" />}
                                        label="I want to receive inspiration, marketing promotions and updates via email."
                                    />
                                </Grid> */}
                                </Grid>
                                <br />
                                <Button
                                    type="submit"
                                    fullWidth
                                    variant="contained"
                                    color="primary"
                                    className={classes.submit}
                                    disabled={!formValid.buttonActive}> Sign Up
                            </Button>
                                <p className="text-danger">{this.props.errorMessage}</p>
                                <p className="text-success">{this.props.successMessage}</p>

                            </form>
                        </div>
                        <Grid container justify="flex-end">
                            <Grid item>
                                <Link to="/login" exact={"true"} >Already have an account? Login here!</Link><br />

                            </Grid>
                        </Grid>
                        <Box mt={5}>
                            <this.Copyright />
                        </Box>

                    </Container>)}
            </Fragment>
        );
    }
}

Register.propTypes = {
    classes: PropTypes.object.isRequired,
};


const mapStateToProps = state => {
    // console.log(state);

    return (
        {
            errorMessage: state.Messages.errorMessage,
            successMessage: state.Messages.successMessage
        })
}

//   export default connect(mapStateToProps)(Register);
export default connect(mapStateToProps)(withStyles(styles)(Register));
